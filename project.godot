[application]

config/name="Lumencraft"
config/description="Lumencraft is a top-down, rogue-like shooter with base building and tower defense elements. Enjoy procedurally generated levels with fully destructible environment."
run/main_scene="res://Scenes/FakeSplashScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="2Dynamic/Lumencraft"
config/features=PackedStringArray("4.4")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Resources/Misc/SplashScreen3.png"
config/icon="res://icon.png"
config/macos_native_icon="res://icon.png"
config/windows_native_icon="res://icon.ico"
run/main_scene.early_access="res://Scenes/SplashScreen.tscn"

[audio]

buses/default_bus_layout="uid://4oeick2pnetv"

[autoload]

Const="*res://Scripts/Singleton/Constants.tscn"
Utils="*res://Scripts/Singleton/Utils.gd"
Save="*res://Scripts/Singleton/Save.gd"
Music="*res://Scripts/Singleton/Music.gd"
PathFinding="*res://Scripts/System/PathFinding.gd"
SteamAPI="*res://Scripts/System/SteamDummy.gd"

[debug]

gdscript/warnings/unassigned_variable=false
gdscript/warnings/unassigned_variable_op_assign=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_signal=false
gdscript/warnings/unreachable_code=false
gdscript/warnings/incompatible_ternary=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false
gdscript/warnings/narrowing_conversion=false
gdscript/warnings/unused_argument=false
gdscript/warnings/function_may_yield=false

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/initial_position_type=3
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/handheld/orientation="sensor_landscape"
window/vsync/vsync_mode=false
mouse_cursor/tooltip_position_offset=Vector2(40, 40)

[dotnet]

project/assembly_name="Lumencraft"

[editor]

search_in_file_extensions=PackedStringArray("gd", "shader", "tscn")

[editor_plugins]

enabled=PackedStringArray("res://addons/DataExplorer/plugin.cfg", "res://addons/SliderLabel/plugin.cfg", "res://addons/SpritesheetGenerator/plugin.cfg")

[gui]

common/snap_controls_to_pixels=false
theme/custom="res://Resources/Anarchy/Themes/theme_anarchy.tres"

