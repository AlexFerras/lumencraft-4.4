script/source = "extends \"res://Scripts/MapEvents.gd\"

func _config_map(config: Dictionary):
	Save.clones = 10

func _ready() -> void:
	map.wave_manager.connect(\"wave_defeated\", Callable(self, \"on_end_wave\"))
	
	if CustomRunner.is_custom_running():
		Utils.game.main_player.add_item(Const.ItemIDs.SPEAR)

func _game_start():
	map.scoring_rules.monster_nests_destroyed = 10000
	Utils.game.ui.set_objective(0, \"Defeat all waves.\")

func on_end_wave():
	if map.wave_manager.is_finished():
		SteamAPI.unlock_achievement(\"WIN_1\")
		Utils.game.win(\"You won! Game will finish in 2 minutes.\")
		get_tree().create_timer(120, false).connect(\"timeout\", Callable(Utils.game.ui, \"show_result\").bind(true))
"

