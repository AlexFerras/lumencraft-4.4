resource_name = "Objectives"
script/source = "extends Node2D

@onready var grubas = get_node_or_null(\"../LowerArea/Finale/EnemyCrystalUrasectus\")

@export var wait_wave: int

func _ready() -> void:
	Utils.game.map.wave_manager.connect(\"wave_defeated\", Callable(self, \"on_wave_defeat\"))
	Utils.connect(\"exploded_terrain\", Callable(self, \"first_drill\").bind(), CONNECT_ONE_SHOT)
	
	if grubas:
		grubas.set_process(false)

func first_wave():
	Utils.game.ui.set_objective(10, \"Fend off a wave of enemies.\")
	wait_wave = 1

func on_wave_defeat():
	if wait_wave == 1:
		Utils.game.ui.set_objective(20, \"Find a way to the cave south-west (SW).\")
		var marker = get_node_or_null(\"../LowerArea/DeadEnd/Marker\")
		if marker:
			marker.show()
	elif wait_wave == 2:
		SteamAPI.unlock_achievement(\"WIN_3\")
		if Utils.game.map.wave_manager.current_wave < 3 or (Utils.game.map.wave_manager.current_wave == 3 and Utils.game.map.wave_manager.wave_countdown > 0):
			SteamAPI.unlock_achievement(\"WIN_FAST\")
		Utils.game.win()
	wait_wave = 0

func first_drill():
	Utils.game.ui.set_objective(0, \"Start the reactor.\")

func south_enter(player) -> void:
	$\"../LowerArea/DeadEnd/Marker\".queue_free()
	Utils.game.ui.set_objective(60, \"Kill the big monster.\")
	grubas.set_process(true)
	grubas.get_node(\"Marker\").show()

func _on_GRUBAS_died() -> void:
	Utils.game.ui.set_objective(70, \"Fend off a wave of enemies.\")
	Utils.game.map.wave_manager.skip_waves(1)
	SteamAPI.unlock_achievement(\"KILL_GRUBAS\")
	wait_wave = 2
"

