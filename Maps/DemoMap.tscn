[sub_resource type="GDScript" id=9]
script/source = "extends Node2D

@onready var pixel_map := Utils.game.map.pixel_map

var dead: bool
@onready var stored_position = get_parent().position

var timer = 0.0
func _physics_process(delta):
	timer += delta
	get_parent().position = stored_position + Vector2(sin(timer), cos(timer*0.341))*2.0

func check():
	if dead:
		return
	
	if not pixel_map.is_pixel_solid(global_position):
		dead = true
		get_parent().is_static = false
		get_parent().create_tween().tween_property(get_parent(), \"modulate:a\", 0.0, 1)
"

[sub_resource type="GDScript" id=16]
resource_name = "FakeComputer"
script/source = "extends Sprite2D

@export var reactor: NodePath
@onready var core = get_node(reactor)
@onready var workshop = $\"../WeaponWorkshop\"
@onready var label: Label = $CenterContainer/VBoxContainer/Label
var timer := 0.0


var player_in: bool

func _ready() -> void:
	Utils.game.map.pixel_map.update_material_mask_rotated(global_position, texture.get_data(), Const.Materials.STOP, Vector3(scale.x, scale.y, global_rotation), 0xFFFFFFFF, 255)

func _on_PlayerDetector_player_entered(player) -> void:
	player.interactables.append(self)

func _on_PlayerDetector_player_exited(player) -> void:
	player.interactables.erase(self)

func _physics_process(delta):
	timer += delta
	$CenterContainer/VBoxContainer.modulate.a = abs( sin(timer) )

func on_interacted(player) -> void:
	if player.get_item_count(Const.ItemIDs.LUMEN) < 10:
		Utils.play_sample(\"res://SFX/Building/InterfaceFail.wav\")
		Utils.game.ui.set_objective(1, \"Look around and find 10 Lumens to start the reactor.\", true)
		return
	else:
		player.subtract_item(Const.ItemIDs.LUMEN, 10)
	
	set_physics_process(false)
	$CenterContainer/VBoxContainer.modulate.a = 1.0
	$CenterContainer/VBoxContainer/HBoxContainer.queue_free()
	
	Utils.play_sample(\"res://SFX/Building/InterfaceAccept.wav\")
	player.interactables.clear()
	player.interactable_icon.call_deferred(\"hide\")
	player.angular_velocity = 0
	player.linear_velocity = Vector2()
	player.set_deferred(\"rotation\", 0)
	player.torso.set_deferred(\"rotation\", -PI * 0.5)
	
	$PlayerDetector.queue_free()
	$\"../../../Start\".queue_free()
	$\"!\".queue_free()
	
	label.text = \"Starting...\"
	label.modulate = Color.YELLOW
	
	var seq := create_tween()
	seq.tween_callback(Callable(Utils.game.ui, \"start_cutscene\"))
	seq.tween_property(Utils.game.camera, \"is_camera_following\", false,0.0)
	seq.tween_interval(0.5)
	seq.tween_property(Utils.game.camera, \"target_position\", core.global_position, 0.5).set_trans(Tween.TRANS_SINE)
	seq.tween_callback(Callable(self, \"start_reactor\"))
	seq.tween_method(Callable(self, \"reveal_core\"), 100, 300, 2.33333)
	seq.tween_interval(1)
	if is_instance_valid(workshop):
		seq.tween_property(Utils.game.camera, \"target_position\", workshop.global_position, 0.9).set_trans(Tween.TRANS_SINE)
		seq.tween_method(Callable(self, \"reveal_workshop\"), 50, 200, 1)
		seq.tween_interval(1)
	seq.tween_property(Utils.game.camera, \"is_camera_following\", true,0.0)
	seq.tween_callback(Callable(Utils.game.ui, \"finish_cutscene\"))
	seq.tween_interval(2)
	seq.tween_callback(Callable($\"../../../Objectives\", \"first_wave\"))

func reveal_core(size):
	Utils.game.map.pixel_map.fog_of_war.clear_circular_area(core.global_position,size, Color(1,1,1,0.03))

func reveal_workshop(size):
	Utils.game.map.pixel_map.fog_of_war.clear_circular_area(workshop.global_position,size, Color(1,1,1,0.06))
	
func start_reactor():
	core.start()
	$LightSprite.queue_free()
	$AnimationPlayer.queue_free()
	
	await core.get_node(\"AnimationPlayer\").animation_finished

	label.text = \"Running\"
	label.modulate = Color.GREEN

"

