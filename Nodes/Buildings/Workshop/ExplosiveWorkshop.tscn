script/source = "extends \"res://Nodes/Buildings/Workshop/Workshop.gd\"

var making: bool

@onready var bar_animator := $Animators/BarAnimator as AnimationPlayer
@onready var add_here := $AddHere as Node2D

@onready var fire1 := $Sprite2D/Fire/AnimationPlayer
@onready var fire2 := $Sprite2D/Fire/AnimationPlayer2

func initialize():
	add_cost(Const.ItemIDs.METAL_SCRAP, 15)
	add_tech_requirement(\"barrels_possible\")
	add_custom_stand(preload(\"res://Nodes/Objects/Explosive/ExplosiveBarrel.tscn\"), preload(\"res://Nodes/Objects/Explosive/ExplosiveBarrel.png\"))
	
	add_stand(Const.ItemIDs.GRENADE, null, 1)
	
	add_stand(Const.ItemIDs.DYNAMITE, null, 1)
	
	add_stand(Const.ItemIDs.MINE, null, 4)
	
	add_stand(Const.ItemIDs.NAPALM, null, 100)

func make_anim():
	making = true
	bar_animator.play(\"Make\", -1, speed_modifier)
	await bar_animator.animation_finished
	bar_animator.play(\"End\", -1, speed_modifier)
	await bar_animator.animation_finished
	making = false
	
	fire1.play(\"RESET\")
	fire2.play(\"RESET\")
	
	finish_make()
	emit_signal(\"full_complete\")

func get_current_progress() -> float:
	var time: float
	var len1: float = bar_animator.get_animation(\"Make\").length
	var len2: float = bar_animator.get_animation(\"End\").length
	
	if making:
		time = bar_animator.current_animation_position
		if bar_animator.current_animation == \"End\":
			time += len1
	else:
		time += len1
		time += len2
	return time / speed_modifier

func get_max_progress() -> float:
	var time: float = bar_animator.get_animation(\"Make\").length
	time += bar_animator.get_animation(\"End\").length
	return time / speed_modifier

func get_pickup_start() -> Vector2:
	return add_here.global_position

func get_pickup_flow_direction() -> Vector2:
	return Vector2.DOWN

func set_disabled(disabled: bool, force := false):
	
	if disabled:
		$LightsAnimator.play(\"PowerOFF\")
	else:
		$LightsAnimator.play(\"PowerON\")
	
	if force: ## nie dzia≈Ça?
		$LightsAnimator.advance(99999)
		
		
#	yield($AnimationPlayer, \"animation_finished\")
	super.set_disabled(disabled, force)
	
#	$LightSprite.visible = not disabled
"

