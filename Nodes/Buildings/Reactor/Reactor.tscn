[sub_resource type="GDScript" id=44]
resource_name = "Clones"
script/source = "extends \"res://Nodes/Buildings/Common/Computer/GenericComputer.gd\"
tool
@onready var life_counter := $Label as Label

var hubbed: bool

func _ready() -> void:
	hubbed = Save.is_hub()

func _setup():
	get_tree().create_timer(0.1).connect(\"timeout\", Callable(self, \"refresh_lifes\"))
	set_display_icon(preload(\"res://Nodes/Buildings/HeroCenter/life_icon.png\"))
	
	if active:
		screen.set_title(\"Create Clone\")
		screen.set_description(\"Create a spare clone, which will be used to revive after death.\")
		
		if not hubbed:
			screen.set_interact_action(\"Create\")
			screen.add_cost(Const.ItemIDs.LUMEN, 100)

func refresh_lifes():
	if not hubbed:
		life_counter.text = str(Save.clones)

func _make():
	Save.clones += 1
	SteamAPI.increment_stat(\"Clones\")
	reload()

func set_disabled(d: bool):
	super.set_disabled(d)
	$Label.visible = not d

func _can_use() -> bool:
	return not hubbed
"

