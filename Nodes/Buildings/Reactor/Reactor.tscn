script/source = "extends \"res://Nodes/Buildings/Common/Computer/GenericComputer.gd\"
tool
@onready var emp = $\"../../../Emp\"
var emp_count =0

func _setup():
	if active:
		screen.set_icon(preload(\"res://Nodes/Buildings/Reactor/emp_icon.png\"))
		screen.set_title(\"Defense Shockwave\")
		screen.set_description(\"Turn your lumens into shockwave to defend your base. Use with caution!\")
		screen.set_interact_action(\"Explode\")
		screen.add_cost(Const.ItemIDs.LUMEN, pow(2,emp_count)*100)

func _make():
	emp.emp_start()
	emp_count+=1
	reload()

func _can_use() -> bool:
	return not emp.anim.is_playing()
	
"

[sub_resource type="GDScript" id=43]
resource_name = "LevelUpgrade"
script/source = "extends \"res://Nodes/Buildings/Common/Computer/GenericComputer.gd\"
tool

const MAX_HP_PER_LEVEL = 1000
const RANGE_PER_LEVEL = 40

@onready var core = owner
@onready var range_upgrade_visual = $\"%range_upgrade_visual\"
@onready var range_visual_circle_minimap = $\"%range_visual_circle_minimap\"

var initial_cost = 0


func _setup():
	if screen.active and active:
		Utils.game.map.post_process.start_build_mode(global_position)
		range_upgrade_visual.visible = true
		screen.set_icon(preload(\"res://Nodes/Buildings/Reactor/range_icon.png\"))
		screen.set_title(\"Upgrade Reactor Level\")
		var text := tr(\"Upgrading reactor's level will increase its health and range. It also increases the maximum number of placed turrets.\\nMax health: %s -> %s\\nMax Turrets: %s -> %s\")
		screen.set_description(text % [core.max_hp, core.max_hp + MAX_HP_PER_LEVEL, BaseBuilding.get_max_turrets(), BaseBuilding.get_max_turrets(core.level + 1)])
		screen.set_interact_action(\"Upgrade\")
		screen.add_cost(Const.ItemIDs.LUMEN, core.level * 50 + initial_cost)
	
		var reactor_range=core.get_meta(\"range_expander_radius\")
		BaseBuilding.init_range_extender_node(range_upgrade_visual,reactor_range+RANGE_PER_LEVEL ,1.0)
		range_visual_circle_minimap.visible = true
		range_visual_circle_minimap.circle_radius=reactor_range
		range_upgrade_visual.circle_radius=reactor_range+RANGE_PER_LEVEL
		range_upgrade_visual.remove_from_group(\"range_expander\")

func _make():
	core.level += 1
	core.RANGE += RANGE_PER_LEVEL
	core.max_hp += MAX_HP_PER_LEVEL
	core.hp += MAX_HP_PER_LEVEL
	core.set_meta(\"range_expander_radius\", core.RANGE)
	Utils.game.map.post_process.range_dirty = true
	reload()

func _uninstall():
#	if screen.active: ## TODO: wcześniej to działało, nie chce mi się szukać czemu
	if range_upgrade_visual.visible:
		Utils.game.map.post_process.stop_build_mode(global_position)
		range_upgrade_visual.visible = false
		range_visual_circle_minimap.visible = false
		Utils.game.map.post_process.range_dirty = true
"

[sub_resource type="GDScript" id=55]
resource_name = "Extractor"
script/source = "extends \"res://Nodes/Buildings/Common/Computer/GenericComputer.gd\"
tool
@onready var core = $\"../../..\"

func _setup():
	if active:
		screen.set_title(\"Extract Lumen\")
		screen.set_description(\"Search for Lumen under the reactor. This will attract attention of the monsters, making the wave come faster.\")
		screen.set_interact_action(\"Extract\")

func _can_use() -> bool:
	return Utils.game.map.wave_manager.wave_countdown > 16

func _make():
	var new_countdown= max(Utils.game.map.wave_manager.wave_countdown -60,15)
	var diff=(Utils.game.map.wave_manager.wave_countdown - new_countdown)
	Utils.game.map.wave_manager.wave_countdown = new_countdown
	core.lumen_to_spawn += int(randf_range(5, 10) * (diff / 60.0) * core.dig_multiplier)
	var x2=Save.is_tech_unlocked(\"mushroom_more_lumens\")
	for building in Utils.get_tree().get_nodes_in_group(\"lumen_farm\"):
		if building.is_running:
			#3,4,5,5,6
			var additional_lumen=3+building.speed-(1 if building.speed>=3 else 0)
			if x2:
				additional_lumen*=2
			var spawn_pos=building.global_position+Vector2(25,0)
			for i in additional_lumen:
				Utils.game.map.pickables.spawn_premium_pickable_nice(spawn_pos,Const.ItemIDs.LUMEN, Vector2.RIGHT.rotated( randf_range(0.0, TAU)) * randf_range(20, 80))

			
	var additional_metal=int((diff / 60.0)*20)
	for building in Utils.get_tree().get_nodes_in_group(\"miner\"):
		if building.is_running:
			if is_instance_valid(building.vein) and building.vein.count > additional_metal:
				if Utils.game.map.pickables.get_pickables_in_range(building.stale_check.global_position, 30.0).size() < 100:
					building.vein.count -=additional_metal
					for i in additional_metal:
						Utils.game.map.pickables.spawn_premium_pickable_nice(building.where.global_position,building.vein.resource, Vector2.RIGHT.rotated(building.where.rotation + randf_range(-0.3, 0.3)) * randf_range(80, 130))

					Utils.game.map.add_dmg_number().custom(building, \"+\"+str(additional_metal), Color.BURLYWOOD)
	
	
	reload()
"

