[sub_resource type="GDScript" id=39]
script/source = "extends Node2D


# Declare member variables here. Examples:
# var a = 2
# var b = \\\"text\\\"
var emp_radius=0
var max_radius=350
@onready var anim=$AnimationPlayer
@onready var area=$Area2D
@onready var collider=$Area2D/CollisionShape2D
func emp_start():
	anim.play(\"emp\")
	
	
func emp():
	emp_radius=0.1
	collider.disabled=false
	Utils.init_player_projectile(area, area, {damage = 500, keep = true, damage_timeout=2000})
	Utils.game.map.post_process.add_shockwave(global_position, max_radius*2, Color(7,3,40,3.1))


func emp_hub():
	emp_radius=0.1
	collider.disabled=false
	Utils.init_player_projectile(area, area, {damage = 500, keep = true, damage_timeout=2000})
	Utils.game.map.post_process.add_hub_shockwave(global_position, 350*2, Color(7,3,40,1.1))

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _physics_process(delta):
	if emp_radius>0:
	#	var enemy_list = Utils.game.map.enemy_tracker.getTrackingNodes2DInCircle(global_position, emp_radius, true)
	#	for enemy in enemy_list:
	#		enemy.take_damage(self)
		emp_radius+=delta*380
		collider.shape.radius=emp_radius
		
		if emp_radius>max_radius:
			emp_radius=0.0
			collider.disabled=true
			collider.shape.radius=emp_radius
"

