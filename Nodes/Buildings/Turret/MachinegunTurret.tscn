script/source = "extends \"res://Nodes/Buildings/Turret/GunTurret.gd\"

@onready var audio  := $Top/ShootPoint/AudioStreamPlayer2D as AudioStreamPlayer2D
@onready var audio2 := $BarrelLoop as AudioStreamPlayer2D
@onready var muzzle_flash := $Top/ShootPoint/MuzzleFlash
@onready var muzzle_spin  := $AnimationPlayer2
@onready var spotlight := $Top/ShootPoint/FlashLight2
@onready var circle_light := $FlashLight

var pierce: int
var damage_lvl: int
var antiair_lvl: int =0

func _no_target():
	muzzle_spin.spin_down()
	audio.reset()
	audio2.stop()
	muzzle_flash.stop()

func _target_found():
	muzzle_spin.spin_up()
	audio2.start()
	shoot_timer = max(shoot_timer - audio2.stream.get_length(), 0.0)

func _shoot():
	muzzle_flash.shoot()
	audio.shoot()
	var bullet := preload(\"res://Nodes/Buildings/Turret/MachinegunBullet.tscn\").instantiate() as Node2D
	bullet.rotation = angle_to_target+randf_range(-0.05, 0.05)
	bullet.global_position = shoot_point.global_position
	bullet.pierce = pierce
	bullet.damage = damage_lvl*2+2
	bullet.antiair = antiair_lvl*2
	bullet.max_distance=max_range+10
	Utils.game.map.add_child(bullet)
	bullet.speed = projectile_speed

	Utils.game.map.pixel_map.flesh_manager.spawn_shell(shoot_point.global_position, -Vector2.UP.rotated(top.rotation) * 80)
	get_tree().create_timer(0.3).connect(\"timeout\", Callable(Utils, \"play_sample\").bind(Utils.random_sound(\"res://SFX/Bullets/bullet_shell_bounce_concrete1\"), self))

func _refresh_upgrades(upgrades: Dictionary):
	shoot_delay = base_shoot_delay - upgrades.fire_rate * 0.08
	pierce = upgrades.pierce
	damage_lvl=upgrades.damage
	antiair_lvl=upgrades.antiair
	match upgrades.fire_rate:
		1:
			audio2.start_samples[0] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_med_start.wav\")
			audio2.loop_samples[0] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_med_loop.wav\")
			audio2.end_samples[0] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_med_stop_01.wav\")
			audio2.end_samples[1] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_med_stop_02.wav\")
			audio2.next_sample()
		2:
			audio2.start_samples[0] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_fast_start.wav\")
			audio2.loop_samples[0] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_fast_loop.wav\")
			audio2.end_samples[0] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_fast_stop_01.wav\")
			audio2.end_samples[1] = preload(\"res://SFX/Turret/gun_auto_barrel_spin_fast_stop_02.wav\")
			audio2.next_sample()

func set_disabled(disabled: bool, force := false):
	super.set_disabled(disabled, force)
#	if setup_complete:
#		laser.visible = not disabled
#	spotlight.visible = not disabled
#	circle_light.visible = not disabled

	if disabled:
		_no_target()
"

