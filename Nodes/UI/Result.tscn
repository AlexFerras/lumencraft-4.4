script/source = "extends CanvasLayer

var win: bool

func _ready() -> void:
	Utils.game.set_physics_process(false)
	Utils.game.disable_music = true
	Music.stop()
	if win:
		$\"%MainLabel\".text = \"Victory!\"
		$\"%AnimationPlayer\".play(\"Display_victory\")
		Utils.play_sample(preload(\"res://SFX/UI/Win_0.wav\"))
	else:
		$\"%MainLabel\".text = \"Defeat!\"
		$\"%AnimationPlayer\".play(\"Display_defeat\")
		Utils.play_sample(preload(\"res://SFX/UI/cinematic_LowDrone1.wav\"))

func change_scene_to_file():
	get_tree().paused = true
	if Save.story_mode:
		get_tree().change_scene_to_file(Const.TITLE_SCENE)
	elif Utils.editor:
		Utils.game.exit(false)
	else:
		Utils.set_meta(\"scoring_rules\", Utils.game.map.scoring_rules)
		get_tree().change_scene_to_file(\"res://Resources/Anarchy/Scenes/anarchy_summary_screen.tscn\")

func end(anim_name: String) -> void:
	get_tree().paused = false
	queue_free()

func set_death_message(message: String):
	$\"%DefeatLabel\".text = message
"

