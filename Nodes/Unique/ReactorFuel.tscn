[sub_resource type="GDScript" id=26]
script/source = "extends \"res://Nodes/Buildings/Common/Computer/GenericComputer.gd\"

@onready var fuel = get_parent()

func _setup():
	if not active:
		return
	
	if fuel.need_to_start > 0:
		screen.set_title(\"Start Reactor\")
		screen.set_description(tr(\"Need %s more Lumen to start the reactor.\") % fuel.need_to_start)
		screen.set_interact_action(tr(\"Add %s Lumen\") % 1)
	else:
		var add := get_add()
		screen.set_title(\"Add Lumen\")
		screen.set_description(tr(\"Puts up to %s Lumen into the reactor, increases time by up to %s seconds.\") % [add, fuel.lumen_fuel_value * add])
		screen.set_icon(Utils.get_item_icon(Const.ItemIDs.LUMEN))
		screen.set_interact_action(tr(\"Add %s Lumen\") % add)

func get_add() -> int:
	var add := 1
	if fuel.need_to_start <= 0:
		add = min( 5, screen.current_player.get_item_count(Const.ItemIDs.LUMEN) )
		if (fuel.fuel_level + fuel.lumen_fuel_value * add) > fuel.max_fuel:
			add -= floor( ((fuel.fuel_level + fuel.lumen_fuel_value * add) - fuel.max_fuel) / fuel.lumen_fuel_value )
	return add

func _can_use() -> bool:
	return screen.current_player.get_item_count(Const.ItemIDs.LUMEN) > 0 and (fuel.reactor.is_running or fuel.need_to_start > 0)

func _make():
	var add := get_add()
	fuel.add_lumen(add)
	screen.current_player.subtract_item(Const.ItemIDs.LUMEN, add)
	reload()
"

