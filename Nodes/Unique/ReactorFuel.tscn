@onready var checker_pos: Vector2 = $Marker2D.global_position

var check_timer: float

func _physics_process(delta: float) -> void:
	check_timer += delta
	if check_timer >= 0.5:
		check_timer = 0
		if not Utils.game.map.pixel_map.is_pixel_solid(checker_pos, Utils.walkable_collision_mask):
			queue_free()
