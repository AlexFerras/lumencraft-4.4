[sub_resource type="GDScript" id=100]
resource_name = "Topso"
script/source = "extends Sprite2D

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		if is_visible_in_tree():
			get_parent().connect(\"frame_changed\", Callable(self, \"sync_frame\"))
		else:
			get_parent().disconnect(\"frame_changed\", Callable(self, \"sync_frame\"))

func sync_frame():
	texture = get_parent().texture
	hframes = get_parent().hframes
	vframes = get_parent().vframes
	frame = get_parent().frame
"

[sub_resource type="GDScript" id=39]
script/source = "extends Sprite2D

func _ready() -> void:
	update_config()

func update_config():
	scale = Vector2.ONE * Save.config.cursor_scale
"

[sub_resource type="GDScript" id=30]
script/source = "extends TextureProgressBar

@onready var player: Player = get_parent()

func _ready():
	hide()
	
func refresh_hp():
	var hp = float(player.hp) / player.get_max_hp()
	tint_progress = Color.from_hsv((hp * 0.5) - 0.05, 1.0, 1.0) 
	tint_progress.r *= 2.0
	tint_progress.g *= 2.0
	tint_progress.b *= 2.0
	value = hp

func set_enabled(enabled: bool):
	if enabled:
		player.connect(\"hp_changed\", Callable(self, \"refresh_hp\"))
		show()
	else:
		player.disconnect(\"hp_changed\", Callable(self, \"refresh_hp\"))
		hide()
"

[sub_resource type="GDScript" id=32]
resource_name = "Reviver"
script/source = "extends GenericInteractable

func interact(player: Player):
	get_parent().reviver = player

func get_interact_text() -> String:
	return \"Revive\"

func _can_interact() -> bool:
	return can_interact and not get_parent().reviver
"

