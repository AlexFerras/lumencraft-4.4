[sub_resource type="GDScript" id=87]
resource_name = "StartConfig"
script/source = "extends \"res://Nodes/Editor/DataEditor.gd\"

@onready var default_button := $Default as BaseButton
@onready var item_container := $HFlowContainer as Container
@onready var add_item := $\"%AddItem\" as Button
@onready var stats := $HFlowContainer2
@onready var upgrades := $GridContainer
@onready var weapon_upgrades := $\"%WeaponUpgrades\"
@onready var technology := $\"%Technology\"
@onready var buildings := $\"%Buildings\"

var item_prefab: PackedScene
var upgrade_prefab: PackedScene
var tech_prefab: PackedScene

func _ready() -> void:
	item_prefab = Prefab.create($HFlowContainer/Item)
	
	for stat in stats.get_children():
		stat.set_meta(\"default\", stat.get_node(\"SpinBox\").value)
	
	tech_prefab = Prefab.create($\"%TechnologyItem\")
	
	for tech in Const.Technology.values():
		var checkbox := tech_prefab.instantiate()
		checkbox.set_tech(tech)
		technology.add_child(checkbox)
	
	upgrade_prefab = Prefab.create(weapon_upgrades.get_node(\"Upgrade\"))
	
	for id in Const.game_data.upgradable_weapons:
		var upgrade_list: Array = Const.Items[id].upgrades
		
		var hbox := HBoxContainer.new()
		hbox.alignment = BoxContainer.ALIGNMENT_CENTER
		
		var icon := TextureRect.new()
		icon.expand = true
		icon.stretch_mode = TextureRect.STRETCH_KEEP_ASPECT_CENTERED
		icon.texture = Utils.get_item_icon(id)
		get_tree().create_timer(0.1).connect(\"timeout\", Callable(self, \"icon_hack\").bind(icon))
		hbox.add_child(icon)
		
		var label := Label.new()
		label.text = Utils.get_item_name({id = id})
		label.modulate = Color.YELLOW
		hbox.add_child(label)
		
		weapon_upgrades.add_child(hbox)
		
		for upgrade in upgrade_list:
			var object := upgrade_prefab.instantiate()
			object.name = str(id, upgrade.name)
			weapon_upgrades.add_child(object)
			object.set_upgrade(upgrade)
	
	for building in Const.Buildings.values():
		if \"category\" in building:
			var checkbox := CheckBox.new()
			checkbox.name = building.name
			checkbox.tooltip_text = building.name
			checkbox.text = Utils.trim_string(tr(building.name), 24)
			checkbox.button_pressed = true
			buildings.add_child(checkbox)

func on_add() -> Control:
	var item: Control = item_prefab.instantiate()
	item_container.add_child(item)
	item_container.move_child(item, item_container.get_child_count() - 2)
	return item

func set_data(data: Dictionary):
	super.set_data(data)
	
	if item_container.get_child_count() > 1:
		for child in item_container.get_children():
			if child is Button:
				continue
			
			child.queue_free()
	
	for item in data.get(\"inventory\", []):
		var item_control := on_add()
		item_control.set_data(item)
	
	default_button.button_pressed = data.get(\"include_default\", true)
	
	stats.get_node(\"max_hp/SpinBox\").value = data.get(\"stats\", {}).get(\"max_hp\", 100)
	stats.get_node(\"max_stamina/SpinBox\").value = data.get(\"stats\", {}).get(\"max_stamina\", 100)
	stats.get_node(\"clones/SpinBox\").value = data.get(\"stats\", {}).get(\"clones\", 0)
	
	for upgrade in upgrades.get_children():
		upgrade.get_node(\"SpinBox\").value = data.get(\"upgrades\", {}).get(upgrade.name, 0)
	
	for upgrade in weapon_upgrades.get_children():
		if upgrade.get_script():
			upgrade.level.value = data.get(\"weapon_upgrades\", {}).get(upgrade.name, 0)
	
	for tech in technology.get_children():
		tech.value = data.get(\"technology\", {}).get(tech.name, 0)
	
	for build in buildings.get_children():
		if build is CheckBox:
			build.button_pressed = not build.name in data.get(\"disabled_buildings\", [])

func get_data() -> Dictionary:
	var data := {}
	data.include_default = default_button.pressed
	
	data.inventory = []
	for item in item_container.get_children():
		if item is Button:
			continue
		data.inventory.append(item.get_data())
	
	data.stats = {}
	for stat in stats.get_children():
		if stat.get_node(\"SpinBox\").value != stat.get_meta(\"default\"):
			data.stats[stat.name] = stat.get_node(\"SpinBox\").value
	
	data.upgrades = {}
	for upgrade in upgrades.get_children():
		if upgrade.get_node(\"SpinBox\").value > 0:
			data.upgrades[upgrade.name] = upgrade.get_node(\"SpinBox\").value
	
	data.weapon_upgrades = {}
	for upgrade in weapon_upgrades.get_children():
		if upgrade.get_script():
			if upgrade.level.value > 0:
				data.weapon_upgrades[upgrade.name] = upgrade.level.value
	
	data.technology = {}
	for tech in technology.get_children():
		if tech.value > 0:
			data.technology[tech.name] = tech.value
	
	data.disabled_buildings = []
	for build in buildings.get_children():
		if build is CheckBox and not build.pressed:
			data.disabled_buildings.append(build.name)
	
	return data

func clear_data():
	set_data({})

func toggle_all_buildings() -> void:
	for building in buildings.get_children():
		if building is CheckBox:
			building.button_pressed = not building.pressed

func icon_hack(icon: TextureRect):
	icon.custom_minimum_size.x = icon.size.y
"

