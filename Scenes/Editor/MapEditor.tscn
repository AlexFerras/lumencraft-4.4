resource_name = "TerrainItem"
script/source = "extends VBoxContainer

@onready var button := find_child(\"Button\")
@onready var config := find_child(\"Config\")
@onready var texture := find_child(\"TextureRect\")
@onready var label := find_child(\"Label\")

var swap: bool
var base_id: int

func _ready() -> void:
	if swap:
		return
	
	var notes = Utils.editor.user_settings.get(\"terrain_notes\")
	if notes is Array:
		if notes.size() > get_id() and notes[get_id()] is String:
				note_tooltip = notes[get_id()]
				update_tooltip()
	
	button.connect(\"gui_input\", Callable(self, \"guinput\"))

func get_id() -> int:
	return base_id + get_index()

func set_terrain(terrain: String, id: int):
	label.text = terrain
	
	var pixel_map: PixelMap = Utils.editor.pixelmap
	
	if id == Const.Materials.LUMEN:
		texture.texture = pixel_map.material.get_shader_parameter(\"lumen_texture\")
		var color = pixel_map.material.get_shader_parameter(\"lumen_color\")
		texture.modulate = Color(color.x, color.y, color.z)
	elif id == Const.Materials.DEAD_LUMEN:
		texture.texture = pixel_map.material.get_shader_parameter(\"lumen_texture\")
		var color = pixel_map.material.get_shader_parameter(\"lumen_color\")
		texture.modulate = Color(color.y * 0.2, color.x, color.z)
	elif id == Const.Materials.LAVA:
		texture.texture = pixel_map.material.get_shader_parameter(\"magma\")
		texture.modulate = pixel_map.material.get_shader_parameter(\"magma_color\")
	elif id == Const.Materials.TAR:
		var floor_material = load(\"res://Nodes/Map/PixelMapFloor_material.tres\") as ShaderMaterial
		texture.texture = floor_material.get_shader_parameter(\"tar_texture\")
		var color = floor_material.get_shader_parameter(\"tar_color\")
		texture.modulate = Color(color.x, color.y, color.z, color.d)
	else:
		var tex := ImageTexture.new()
		tex.create_from_image(pixel_map.material.get_shader_parameter(\"terrain_texture_mix\").get_layer_data(id))
		texture.texture = tex
		texture.modulate = Color(0.5, 0.5, 0.5)
	
	set_durability_tooltip(pixel_map.material_data.get_material_durability(id))

func set_terrain_mat(terrain: String, mat):
	label.text = terrain
	texture.texture = get_texture_from_image(mat.texture)
	texture.modulate = Color.WHITE
	set_durability_tooltip(mat.durability)

func set_floor(mat, text := \"\"):
	label.text = text
	texture.texture = mat.texture
	texture.material.set_shader_parameter(\"hsv_offset\", mat.hsv)

func set_configurable():
	config.show()

var base_tooltip: String
var note_tooltip: String

func set_durability_tooltip(durability):
	var drill_level := int(ceil(float(durability - 2) / 1.5))
	base_tooltip = \"Durability: %s\\nDrill Level: %s\" % [int(durability), drill_level]
	update_tooltip()

var texture_cache: Dictionary

func get_texture_from_image(image: Image) -> Texture2D:
	if not image in texture_cache:
		var tex := ImageTexture.new()
		tex.create_from_image(image)
		texture_cache[image] = tex
	
	return texture_cache[image]

var popup: PopupMenu
var popup2: PopupPanel
var note_edit: LineEdit

func guinput(event: InputEvent) -> void:
	if event is InputEventMouseButton:
		if event.pressed and event.button_index == MOUSE_BUTTON_RIGHT:
			if not popup:
				popup = PopupMenu.new()
				popup.add_item(\"Set Note\")
				popup.add_item(\"Remove Note\")
				popup.connect(\"id_pressed\", Callable(self, \"on_menu\"))
				add_child(popup)
				
				popup2 = PopupPanel.new()
				add_child(popup2)
				
				note_edit = LineEdit.new()
				note_edit.custom_minimum_size = Vector2(400, 0)
				popup2.add_child(note_edit)
				note_edit.connect(\"text_submitted\", Callable(self, \"on_note_changed\"))
			
			popup.global_position = get_global_mouse_position()
			popup.popup()

func on_menu(id: int):
	if id == 0:
		popup.hide()
		popup2.position = popup.position
		note_edit.text = note_tooltip
		popup2.popup()
		note_edit.call_deferred(\"grab_focus\")
	elif id == 1:
		on_note_changed(\"\")

func update_tooltip():
	button.tooltip_text = \"\"
	if not note_tooltip.is_empty():
		button.tooltip_text += note_tooltip + \"\\n\"
	button.tooltip_text += base_tooltip

func on_note_changed(text: String):
	popup2.hide()
	note_tooltip = text
	update_tooltip()
	
	var notes: Array
	if Utils.editor.user_settings.has(\"terrain_notes\"):
		notes = Utils.editor.user_settings[\"terrain_notes\"]
	else:
		Utils.editor.user_settings[\"terrain_notes\"] = notes
	
	if notes.size() < get_id() + 1:
		notes.resize(get_id() + 1)
	
	notes[get_id()] = note_tooltip
	Utils.editor.save_user_settings()
"

