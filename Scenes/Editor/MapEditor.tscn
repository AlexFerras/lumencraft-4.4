[sub_resource type="GDScript" id=113]
resource_name = "TerrainPreview"
script/source = "extends Node2D

var generated_terrain: RefCounted

func _ready() -> void:
	await Utils.editor.ready
	get_minimap_overlay().connect(\"draw\", Callable(self, \"draw_minimap\"))

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		get_minimap_overlay().update()

func _draw() -> void:
	var map_size: Vector2 = Utils.editor.map_size
	draw_rect(Rect2(Vector2(), Vector2.ONE * map_size), Color.DIM_GRAY)
	
	if generated_terrain:
		draw_texture(generated_terrain.final_texture, map_size * 0.5 - generated_terrain.final_texture.get_size() * 0.5, Color.DIM_GRAY)
	else:
		draw_texture_rect(preload(\"res://Nodes/Editor/Icons/TerrainChecker.png\"), Rect2(Vector2(), Vector2.ONE * map_size), true)

func draw_minimap():
	if not is_visible_in_tree():
		return
	
	var minimap := get_minimap_overlay()
	minimap.draw_rect(Rect2(Vector2(), minimap.size), Color.RED)
	
	if not generated_terrain:
		return
	
	var map_size: Vector2 = Utils.editor.map_size
	minimap.draw_set_transform(Vector2(), 0, get_minimap_overlay().size / map_size)
	minimap.draw_texture(generated_terrain.final_texture, map_size * 0.5 - generated_terrain.final_texture.get_size() * 0.5, Color.RED)

func get_minimap_overlay() -> Control:
	return Utils.editor.minimap_overlay

func set_generated_terrain(terrain: RefCounted):
	generated_terrain = terrain
	if generated_terrain and not generated_terrain.final_texture:
		generated_terrain.bake()
		await generated_terrain.bake_finished
	
	update()
	get_minimap_overlay().update()
"

