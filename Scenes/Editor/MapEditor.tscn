[sub_resource type="GDScript" id=102]
resource_name = "Objectives"
script/source = "extends \"res://Nodes/Editor/DataEditor.gd\"

enum {OBJECTIVE_WAVES, OBJECTIVE_ITEM, OBJECTIVE_FINISH, OBJECTIVE_TIME, OBJECTIVE_BUILDING, OBJECTIVE_TECH, OBJECTIVE_ENEMY, OBJECTIVE_GENOCIDE, OBJECTIVE_SCORE, OBJECTIVE_CUSTOM}

const CONDITION_SETTINGS = {OBJECTIVE_ITEM: \"Item\", OBJECTIVE_FINISH: \"Goal\", OBJECTIVE_TIME: \"Time\", OBJECTIVE_BUILDING: \"Building\", OBJECTIVE_TECH: \"Technology\", OBJECTIVE_ENEMY: \"Enemy\", OBJECTIVE_SCORE: \"Score\", OBJECTIVE_CUSTOM: \"Custom\"}

func _ready() -> void:
	for item in Const.game_data.get_editor_pickup_list():
		$\"%WinItemID\".add_icon_item(Utils.get_item_icon(item.id, item.get(\"data\")), tr(Utils.get_item_name(item)), 12)
		$\"%WinItemID\".set_item_metadata($\"%WinItemID\".get_item_count() - 1, item)
	
	for building in Const.Buildings.values():
		if not \"category\" in building:
			continue
		$\"%WinBuilding\".add_item(building.name)
	
	for enemy in Const.game_data.get_editor_enemy_list():
		if enemy.begins_with(\"Swarm\"):
			$\"%WinEnemy\".add_item(enemy.get_slice(\"/\", 1))
		else:
			$\"%WinEnemy\".add_item(enemy)
	
	for tech in Const.Technology.values():
		$\"%WinTechnology\".add_item(tech.name)
		$\"%WinTechnology\".set_item_metadata($\"%WinTechnology\".get_item_count() - 1, tech.tech)

func set_data(data: Dictionary):
	super.set_data(data)
	
	var win_condition: Dictionary = data.get(\"win\", {})
	if not win_condition.is_empty():
		match win_condition.type:
			\"waves\":
				$\"%WinCondition\".select(OBJECTIVE_WAVES)
			\"item\":
				$\"%WinCondition\".select(OBJECTIVE_ITEM)
				for i in $\"%WinItemID\".get_item_count():
					var item: Dictionary = $\"%WinItemID\".get_item_metadata(i)
					if item.id == win_condition.id and item.get(\"data\") == win_condition.get(\"data\"):
						$\"%WinItemID\".selected = i
						break
				$\"%WinItemAmount\".value = win_condition.amount
			\"finish\":
				$\"%WinCondition\".select(OBJECTIVE_FINISH)
				$\"%WinGoalType\".select(win_condition.get(\"goal_type\", 0))
			\"time\":
				$\"%WinCondition\".select(OBJECTIVE_TIME)
				$\"%WinTime\".set_time(win_condition.time)
			\"building\":
				$\"%WinCondition\".select(OBJECTIVE_BUILDING)
				for i in $\"%WinBuilding\".get_item_count():
					if $\"%WinBuilding\".get_item_text(i) == win_condition.target_building:
						$\"%WinBuilding\".selected = i
						break
			\"technology\":
				$\"%WinCondition\".select(OBJECTIVE_TECH)
				for i in $\"%WinTechnology\".get_item_count():
					if $\"%WinTechnology\".get_item_metadata(i) == win_condition.target_tech:
						$\"%WinTechnology\".selected = i
						break
			\"enemy\":
				$\"%WinCondition\".select(OBJECTIVE_ENEMY)
				for i in $\"%WinEnemy\".get_item_count():
					if $\"%WinEnemy\".get_item_text(i) == win_condition.target_enemy:
						$\"%WinEnemy\".selected = i
						break
				$\"%WinEnemyCount\".value = win_condition.amount
			\"genocide\":
				$\"%WinCondition\".select(OBJECTIVE_GENOCIDE)
			\"score\":
				$\"%WinCondition\".select(OBJECTIVE_SCORE)
				$\"%WinScore\".value = win_condition.amount
			\"custom\":
				$\"%WinCondition\".select(OBJECTIVE_CUSTOM)
				$\"%WinCustomMessage\".text = win_condition.message
	
	$\"%WinAutoFinish\".set_pressed_no_signal(data.get(\"auto_finish\", false))
	
	var fail_conditions: Dictionary = data.get(\"fail\", {})
	$\"%FailReactor\".button_pressed = fail_conditions.get(\"reactor\", true)
	
	if \"time_limit\" in fail_conditions:
		$\"%FailTimeEnabled\".button_pressed = true
		$\"%FailTime\".set_time(fail_conditions.time_limit)
	else:
		$\"%FailTimeEnabled\".button_pressed = false
	
	if \"min_score\" in fail_conditions:
		$\"%FailScore\".button_pressed = true
		$\"%FailScoreValue\".value = fail_conditions.min_score
	else:
		$\"%FailScore\".button_pressed = false
	
	if not \"scoring\" in data:
		data.scoring = {table = {enemies_slain = 5, lumen_collected = 10, metal_collected = 5}}
	
	$\"%StartingScore\".value = data.scoring.get(\"starting_score\", 0)
	$\"%ClearBonus\".value = data.scoring.get(\"clear_bonus\", 25)
	
	var scoring_table: Dictionary = data.scoring.table
	unscoop_score(\"time\", scoring_table)
	unscoop_score(\"enemies_slain\", scoring_table)
	unscoop_score(\"lumen_collected\", scoring_table)
	unscoop_score(\"metal_collected\", scoring_table)
	unscoop_score(\"shots_fired\", scoring_table)
	unscoop_score(\"tech_researched\", scoring_table)
	unscoop_score(\"damage_taken\", scoring_table)
	unscoop_score(\"deaths\", scoring_table)
	unscoop_score(\"goals_reached\", scoring_table)
	
	update_condition()

func get_data() -> Dictionary:
	var data: Dictionary
	data.auto_finish = $\"%WinAutoFinish\".pressed
	
	data.win = {}
	match $\"%WinCondition\".selected:
		OBJECTIVE_WAVES:
			data.win.type = \"waves\"
		OBJECTIVE_ITEM:
			data.win.type = \"item\"
			var item: Dictionary = $\"%WinItemID\".get_item_metadata($\"%WinItemID\".selected)
			data.win.id = item.id
			data.win.data = item.get(\"data\")
			data.win.amount = $\"%WinItemAmount\".value
		OBJECTIVE_FINISH:
			data.win.type = \"finish\"
			data.win.goal_type = $\"%WinGoalType\".selected
		OBJECTIVE_TIME:
			data.win.type = \"time\"
			data.win.time = $\"%WinTime\".get_time()
		OBJECTIVE_BUILDING:
			data.win.type = \"building\"
			data.win.target_building = $\"%WinBuilding\".get_item_text($\"%WinBuilding\".selected)
		OBJECTIVE_TECH:
			data.win.type = \"technology\"
			data.win.target_tech = $\"%WinTechnology\".get_selected_metadata()
		OBJECTIVE_ENEMY:
			data.win.type = \"enemy\"
			data.win.target_enemy = $\"%WinEnemy\".get_item_text($\"%WinEnemy\".selected)
			data.win.amount = $\"%WinEnemyCount\".value
		OBJECTIVE_GENOCIDE:
			data.win.type = \"genocide\"
		OBJECTIVE_SCORE:
			data.win.type = \"score\"
			data.win.amount = $\"%WinScore\".value
		OBJECTIVE_CUSTOM:
			data.win.type = \"custom\"
			data.win.message = $\"%WinCustomMessage\".text
	
	data.fail = {}
	data.fail.reactor = $\"%FailReactor\".pressed
	
	if $\"%FailTimeEnabled\".pressed:
		data.fail.time_limit = $\"%FailTime\".get_time()
	
	if $\"%FailScore\".pressed:
		data.fail.min_score = $\"%FailScoreValue\".value
	
	data.scoring = {}
	data.scoring.starting_score = $\"%StartingScore\".value
	data.scoring.clear_bonus = $\"%ClearBonus\".value
	
	data.scoring.table = {}
	scoop_score(\"time\", data.scoring.table)
	scoop_score(\"enemies_slain\", data.scoring.table)
	scoop_score(\"lumen_collected\", data.scoring.table)
	scoop_score(\"metal_collected\", data.scoring.table)
	scoop_score(\"shots_fired\", data.scoring.table)
	scoop_score(\"tech_researched\", data.scoring.table)
	scoop_score(\"damage_taken\", data.scoring.table)
	scoop_score(\"deaths\", data.scoring.table)
	scoop_score(\"goals_reached\", data.scoring.table)
	
	return data

func scoop_score(entry: String, table: Dictionary):
	var score: SpinBox = get_node(\"%Scoring|\" + entry)
	if score.value != 0 or entry == \"time\":
		table[entry] = score.value

func unscoop_score(entry: String, table: Dictionary):
	var score: SpinBox = get_node(\"%Scoring|\" + entry)
	score.value = table.get(entry, 0)

func condition_changed(index: int) -> void:
	update_condition()

func update_condition():
	for condition in get_tree().get_nodes_in_group(\"win_settings\"):
		condition.visible = CONDITION_SETTINGS.get($\"%WinCondition\".selected, \"\") == condition.name
"

