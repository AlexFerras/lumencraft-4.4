[sub_resource type="GDScript" id=127]
resource_name = "EditorAction"
script/source = "extends VBoxContainer

@onready var action_list: OptionButton = $ObjectActions/ActionList
@onready var pick_button: Button = $HBoxContainer/Button
@onready var find_button: Button = $HBoxContainer2/Button
@onready var target_name: Label = $HBoxContainer2/TargetName

var is_action: bool
var object_id: int = -99
var target_object: EditorObject

var extra_options: Control
var is_instant: bool
var registered_properties: Array
var loaded_data: Dictionary

func _ready() -> void:
	action_list.connect(\"item_selected\", Callable(self, \"refresh_extra_options\"))

func set_special_target() -> void:
	pick_object(preload(\"res://Nodes/Editor/SpecialObject.gd\").new())

func get_data() -> Dictionary:
	if object_id >= -1:
		var ret = {id = object_id, type = action_list.get_selected_metadata()}
		
		var extra_data: Dictionary = get_extra_data()
		if not extra_data.is_empty():
			ret.data = extra_data
		
		return ret
	
	return {}

func set_data(data: Dictionary):
	loaded_data = data.get(\"data\", {})
	
	if data.id == -1:
		set_special_target()
	else:
		pick_object(Utils.editor.placed.get_child(data.id))
	
	for i in action_list.get_item_count():
		if action_list.get_item_metadata(i) == data.type:
			action_list.selected = i
			break

func pick_object(object):
	cancel_picking()
	
	if target_object:
		target_object.disconnect(\"deleted\", Callable(self, \"pick_object\"))
		target_object = null
	
	action_list.clear()
	if object == null:
		target_name.text = \"\"
		object_id = -99
		find_button.hide()
		refresh_extra_options()
		return
	
	object_id = object.get_id()
	if object is EditorObject:
		target_object = object
		target_object.connect(\"deleted\", Callable(self, \"pick_object\").bind(null))
		find_button.show()
	else:
		find_button.hide()
	
	var list: Array = object.action_get_events() if is_action else object.get_condition_list()
	for item in list:
		var instant: bool
		if item.ends_with(\"*\"):
			instant = true
			item = item.trim_suffix(\"*\")
		
		if instant:
			action_list.add_icon_item(preload(\"res://Nodes/Editor/Icons/Instant.png\"), item.capitalize())
			action_list.set_item_tooltip(action_list.get_item_count() - 1, \"Instant (not fullfiled continuously)\")
		else:
			action_list.add_item(item.capitalize())
		action_list.set_item_metadata(action_list.get_item_count() - 1, item)
	
	target_name.text = object.get_short_target_name()
	target_name.tooltip_text = object.get_target_name()
	action_list.disabled = false
	
	call_deferred(\"refresh_extra_options\")

func start_target_pick() -> void:
	Utils.editor.start_target_pick(self)
	pick_button.modulate = Color.YELLOW_GREEN
	
func cancel_picking():
	pick_button.modulate = Color.WHITE

func delete_action() -> void:
	queue_free()

func copy_action() -> void:
	if is_action:
		Utils.set_meta(\"action_clipboard\", get_data())
		get_tree().call_group(\"action_paste\", \"set_disabled\", false)
	else:
		Utils.set_meta(\"condition_clipboard\", get_data())
		get_tree().call_group(\"condition_paste\", \"set_disabled\", false)

func find_object() -> void:
	Utils.editor.center_on_object(target_object)

func hover_find() -> void:
	Utils.editor.locate_object = target_object

func hover_stop() -> void:
	Utils.editor.locate_object = null

func refresh_extra_options(whatever = null):
	if extra_options:
		extra_options.queue_free()
		extra_options = null
		registered_properties.clear()
	
	var option_object = target_object
	if not option_object and object_id >= -1:
		option_object = preload(\"res://Nodes/Editor/SpecialObject.gd\").new()
	
	if option_object:
		extra_options = option_object.get_additional_config(self, action_list.get_selected_metadata())
		if extra_options:
			add_child(extra_options)
	
	if not is_action:
		set_meta(\"instant\", action_list.get_item_icon(action_list.selected) != null)
		get_event().update_repeatable()

func register_data(property: String, setter: FuncRef, getter: FuncRef, default = null):
	registered_properties.append({name = property, setter = setter, getter = getter})
	
	await get_tree().idle_frame
	
	if property in loaded_data:
		setter.call_func(loaded_data.get(property, default))

func get_extra_data() -> Dictionary:
	var data: Dictionary
	for property in registered_properties:
		data[property.name] = property.getter.call_func()
	
	return data

func reassign_id():
	if target_object:
		object_id = target_object.get_index()

func get_event() -> Node:
	return get_parent().get_parent().get_parent().get_parent()
"

