[sub_resource type="GDScript" id=137]
resource_name = "PickupList"
script/source = "extends PanelContainer

const CONTAINER_SCRIPT = preload(\"res://Nodes/Editor/Objects/EditorItemContainer.gd\")

@onready var container: VBoxContainer = $\"%PickupInfoContainer\"

var info_prefab: PackedScene

func _ready() -> void:
	info_prefab = Prefab.create($\"%PickupInfoPrefab\")
	hide()

func appear():
	for i in container.get_child_count() - 1:
		container.get_child(i + 1).queue_free()
	
	var pickup_infos: Array
	
	for object in owner.placed.get_children():
		if object.object_name == \"Pickup\":
			pickup_infos.append([{id = object.object_data.id, data = object.object_data.get(\"data\"), amount = object.object_data.amount}, object])
		elif object.object_name == \"Technology Orb\":
			pickup_infos.append([{id = Const.ItemIDs.TECHNOLOGY_ORB, data = object.object_data, amount = 1}, object])
		elif object is CONTAINER_SCRIPT:
			if object.object_name == \"Stone Gate\":
				continue
			
			for item in object.object_data.items:
				pickup_infos.append([{id = item.id, data = item.get(\"data\"), amount = item.amount}, object])
	
	pickup_infos.sort_custom(Callable(self, \"sort_infos\"))
	
	for info in pickup_infos:
		callv(\"add_pickup_info\", info)
	
	show()

func disappear() -> void:
	if not $\"%PickupListOpen\".is_visible_in_tree():
		hide()

func sort_infos(info1, info2) -> bool:
	return info1[0].id < info2[0].id

func add_pickup_info(item: Dictionary, source: EditorObject):
	var info = info_prefab.instantiate()
	info.get_node(\"TextureRect\").texture = Utils.get_item_icon(item.id, item.data)
	info.get_node(\"Button\").connect(\"pressed\", Callable(owner, \"center_on_object\").bind(source))
	container.add_child(info)
	
	if source.object_name == \"Pickup\" or source.object_name == \"Technology Orb\":
		info.get_node(\"Label\").text = \"%s x %s\" % [tr(Utils.get_item_name(item)), item.amount]
	else:
		info.get_node(\"Label\").text = \"%s x %s (%s)\" % [tr(Utils.get_item_name(item)), item.amount, tr(\"inside %s\") % tr(source.object_name)]
"

[sub_resource type="GDScript" id=135]
resource_name = "Publishing"
script/source = "extends Control

@onready var map_id: Label = $CenterContainer/VBoxContainer/ID
@onready var map_preview: TextureRect = $CenterContainer/VBoxContainer/ColorRect/MarginContainer/Preview
@onready var update_note: LineEdit = $CenterContainer/VBoxContainer/Note
@onready var preview_edit: LineEdit = $CenterContainer/VBoxContainer/CustomPreview/LineEdit
@onready var preview_picker: FileDialog = $CenterContainer/VBoxContainer/CustomPreview/Button/FileDialog
@onready var publish_button: Button = $CenterContainer/VBoxContainer/Publish
@onready var publish_status: Control = $CenterContainer/VBoxContainer/Status
@onready var tos: Control = $CenterContainer/VBoxContainer/TOS
@onready var snapshot: Control = $SubViewport/Snapshot

var internal_tag_prefab: PackedScene

var workshop_id: int

var type_tag_group: ButtonGroup
var difficulty_tag_group: ButtonGroup
var length_tag_group: ButtonGroup

func _ready() -> void:
	internal_tag_prefab = Prefab.create($\"%InternalTagPrefab\")
	
	if SteamAPI.active:
		SteamAPI.connect(\"publish_status\", Callable(self, \"on_publish_status\"))
		SteamAPI.connect(\"need_tos\", Callable(self, \"on_need_tos\"))
	
	type_tag_group = $\"%WorkshopMapType\".group
	difficulty_tag_group = $\"%WorskhopDifficulty\".group
	length_tag_group = $\"%WorkshopLength\".group

func setup():
	map_preview.texture = null
	generate_preview()
	publish_status.text = \"\"
	tos.hide()
	
	var metadata = owner.get_internal_tags()
	for tag in $\"%InternalTags\".get_children():
		tag.queue_free()
	
	for tag in metadata:
		var tag_button = internal_tag_prefab.instantiate()
		tag_button.text = Utils.get_internal_tag_name(tag)
		$\"%InternalTags\".add_child(tag_button)
	
	if workshop_id > -1:
		map_id.show()
		map_id.text = \"ID: %s\" % workshop_id
	else:
		map_id.hide()
		return
	
	SteamAPI.query_item_tags(workshop_id, funcref(self, \"tag_callback\"))
	
	for button in type_tag_group.get_buttons():
		button.disabled = true
	for button in difficulty_tag_group.get_buttons():
		button.disabled = true
	for button in length_tag_group.get_buttons():
		button.disabled = true

func tag_callback(tags: PackedStringArray):
	if type_tag_group.get_pressed_button():
		type_tag_group.get_pressed_button().button_pressed = false
	clear_difficulty()
	clear_length()
	
	for tag in tags:
		if tag.begins_with(\"Difficulty\"):
			var text = tag.get_slice(\": \", 1)
			for button in difficulty_tag_group.get_buttons():
				if button.text == text:
					button.button_pressed = true
					break
		elif tag.begins_with(\"Length\"):
			var text = tag.get_slice(\": \", 1)
			for button in length_tag_group.get_buttons():
				if button.text == text:
					button.button_pressed = true
					break
		else:
			for button in type_tag_group.get_buttons():
				if button.text == tag:
					button.button_pressed = true
					break
	
	for button in type_tag_group.get_buttons():
		button.disabled = false
	for button in difficulty_tag_group.get_buttons():
		button.disabled = false
	for button in length_tag_group.get_buttons():
		button.disabled = false

func generate_preview():
	snapshot.texture = owner.pixelmap.get_texture()
	snapshot.get_parent().render_target_update_mode = SubViewport.UPDATE_ONCE
	await get_tree().idle_frame
	await get_tree().idle_frame
	map_preview.texture = snapshot.get_parent().get_texture()

func on_publish_confirm() -> void:
	publish_status.modulate = Color.GOLD
	publish_status.text = \"Publishing...\"
	
	if workshop_id > -1:
		do_publish()
	else:
		SteamAPI.connect(\"item_created\", Callable(self, \"on_map_created\").bind(), CONNECT_ONE_SHOT)
		SteamAPI.create_map()

func on_pick_preview() -> void:
	preview_picker.popup_centered()

func on_preview_selected(dir: String) -> void:
	preview_edit.text = dir
	on_preview_changed(dir)

func on_preview_changed(new_text: String) -> void:
	if not validate_preview_file(new_text):
		preview_edit.modulate = Color.RED
		map_preview.texture = null
	else:
		preview_edit.modulate = Color.WHITE
		if new_text.is_empty():
			map_preview.texture = snapshot.get_parent().get_texture()
		else:
			map_preview.texture = SteamAPI.load_preview_image(new_text)

func validate_preview_file(file: String) -> bool:
	if file.is_empty():
		return true
	
	if not file.get_extension() in [\"png\", \"jpg\", \"gif\"]:
		return false
	
	var f := File.new()
	if not f.file_exists(file):
		return false
	
	return true

func on_need_tos():
	tos.show()

func open_tos() -> void:
	OS.shell_open(\"https://steamcommunity.com/sharedfiles/workshoplegalagreement\")
	tos.hide()

func on_map_created(result: int, id: int):
	if result != 1:
		publish_status.modulate = Color.RED
		var error: String
		match result:
			24:
				error = \"Upload restricted. Contact Steam Support\"
			17:
				error = \"Banned\"
			16:
				error = \"Request timed out. Try again\"
			21:
				error = \"Not logged in\"
			20:
				error = \"Service unavailable. Try again\"
			8:
				error = \"Invalid parameter\"
			15:
				error = \"Access denied\"
			25:
				error = \"Workshop upload quota exceeded. Remove some files\"
			9:
				error = \"File not found\"
			29:
				error = \"Already created\"
			14:
				error = \"Name already used\"
			44:
				error = \"Account restricted due to password change\"
			_:
				error = \"Unknown error\"
		
		publish_status.text = tr(\"Failed to create item. Error %s: %s\") % [result, tr(error)]
		return
	
	workshop_id = id
	map_id.show()
	map_id.text = \"ID: %s\" % workshop_id
	
	if not owner.try_publish():
		publish_status.modulate = Color.WHITE
		publish_status.text = \"Save before publishing\"
		return
	
	do_publish()

func do_publish() -> void:
	DirAccess.new().make_dir_recursive(\"user://workshop/published_map\")
	owner.save_map(\"user://workshop/published_map/map.lcmap\")
	
	var preview_path := ProjectSettings.globalize_path(\"user://workshop/published_preview.png\")
	if preview_edit.text.is_empty():
		var preview: Image = map_preview.texture.get_data().duplicate()
		preview.shrink_x2()
		preview.save_png(preview_path)
	else:
		preview_path = preview_edit.text
	
	var tags: PackedStringArray
	tags.append(type_tag_group.get_pressed_button().text)
	if difficulty_tag_group.get_pressed_button():
		tags.append(\"Difficulty: \" + difficulty_tag_group.get_pressed_button().text)
	if length_tag_group.get_pressed_button():
		tags.append(\"Length: \" + length_tag_group.get_pressed_button().text)
	
	var metadata: String = owner.\",\".join(get_internal_tags())
	
	SteamAPI.publish_map(workshop_id, owner.map_name.text, owner.map_description.text, update_note.text, preview_path, tags, metadata)

func on_publish_status(result: int):
	if result == 1:
		update_note.text = \"\"
		publish_status.modulate = Color.GREEN
		publish_status.text = \"Published successfully!\"
	else:
		publish_status.modulate = Color.RED
		var error: String
		match result:
			8:
				error = \"Invalid parameter\"
			9:
				error = \"File not found or invalid. Make sure the game is not blocked by antivirus\"
			15:
				error = \"Access denied. Make sure you own the game\"
			33:
				error = \"Internal error\"
			25:
				error = \"Preview file too big. Try a different one\"
			_:
				error = \"Unknown error\"
		
		publish_status.text = tr(\"Publishing failed. Error %s: %s\") % [result, tr(error)]
		

func on_publish_cancel() -> void:
	owner.show_publish(false)

func clear_difficulty() -> void:
	if difficulty_tag_group.get_pressed_button():
		difficulty_tag_group.get_pressed_button().button_pressed = false

func clear_length() -> void:
	if length_tag_group.get_pressed_button():
		length_tag_group.get_pressed_button().button_pressed = false
"

