resource_name = "Mousetick"
script/source = "extends Node

func _ready() -> void:
	Input.connect(\"joy_connection_changed\", Callable(self, \"set_joys\"))
	set_joys(1, 2)
	
#	if not Input.get_connected_joypads().empty():
#		for action in [\"ui_left\", \"ui_right\", \"ui_up\", \"ui_down\", \"ui_accept\", \"ui_cancel\"]:
#			InputMap.action_erase_events(action)

func set_joys(a, b):
	set_process_input(not Input.get_connected_joypads().is_empty())
	set_process(not Input.get_connected_joypads().is_empty())

func _physics_process(delta: float) -> void:
	var joy_move := Input.get_vector(\"p3_left\", \"p3_right\", \"p3_up\", \"p3_down\")
	if joy_move != Vector2():
		Input.warp_mouse(get_viewport().get_mouse_position() + joy_move * 1000 * delta)
	
	joy_move = Input.get_vector(\"p3_look_left\", \"p3_look_right\", \"p3_look_up\", \"p3_look_down\")
	if joy_move != Vector2():
		get_parent().set_camera_position(get_parent().camera.position + joy_move * 1000 * delta)

func _input(event: InputEvent) -> void:
	var fake_event: InputEvent
	
	if event is InputEventJoypadButton:
		if event.button_index == JOY_L:
			fake_event = InputEventMouseButton.new()
			fake_event.button_index = MOUSE_BUTTON_LEFT
			fake_event.button_pressed = event.pressed
		elif event.button_index == JOY_R:
			fake_event = InputEventMouseButton.new()
			fake_event.button_index = MOUSE_BUTTON_RIGHT
			fake_event.button_pressed = event.pressed
		elif event.button_index == JOY_R3:
			fake_event = InputEventMouseButton.new()
			fake_event.button_index = MOUSE_BUTTON_MIDDLE
			fake_event.button_pressed = event.pressed
	
	if fake_event:
		if fake_event is InputEventMouse:
			fake_event.global_position = get_viewport().get_mouse_position()
			fake_event.position = fake_event.global_position
			
			if Input.is_joy_button_pressed(0, JOY_BUTTON_A):
				fake_event.command = true
			if Input.is_joy_button_pressed(0, JOY_BUTTON_X):
				fake_event.shift = true
			if Input.is_joy_button_pressed(0, JOY_BUTTON_Y):
				fake_event.alt = true
		
		Input.parse_input_event(fake_event)
"

