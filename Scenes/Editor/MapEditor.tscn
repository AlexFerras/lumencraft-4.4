[sub_resource type="GDScript" id=9]
resource_name = "CameraPosition"
script/source = "extends ReferenceRect

@export var camera_path: NodePath
@onready var camera: Camera2D = get_node(camera_path)

@onready var overlay: Control = $\"%Overlay\"

var camera_dragging: bool

func _ready() -> void:
	set_process(false)

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		set_process(is_visible_in_tree())

func _process(delta: float) -> void:
	var map_size: Vector2 = Utils.editor.map_size
	if map_size == Vector2():
		return
	
	var parent_size: Vector2 = get_parent().size
	size = parent_size / map_size * overlay.size * camera.zoom
	position = camera.get_camera_screen_center() / map_size * get_parent().size - size * 0.5

func _minimap_input(event: InputEvent):
	if event is InputEventMouseButton:
		if event.button_index == MOUSE_BUTTON_LEFT:
			camera_dragging = event.pressed
	
	if camera_dragging:
		var map_size: Vector2 = Utils.editor.map_size
		Utils.editor.set_camera_position(event.position * map_size / get_parent().size)
"

