resource_name = "TerrainConfig"
script/source = "extends VBoxContainer

const SKIP_FLOORS = [\"1WeatheredRock.tres\", \"1Underground1.tres\", \"1Underground2.tres\", \"1Underground3.tres\", \"1Underground4.tres\", \"2Underground1.tres\", \"2Underground2.tres\", \"2Underground3.tres\", \"2Underground4.tres\"]
const SKIP_WALLS = [\"Dirt.tres\"]

@onready var terrain_materials := $\"%TerrainMaterials\"

@onready var surface_floor_textures := $\"%SurfaceFloorTextures\"
@onready var base_floor_textures := $\"%BaseFloorTextures\"
var floor_hardness: Range

var terrain_prefab: PackedScene
var terrain_group: ButtonGroup
var floor_group: ButtonGroup

var selected_material: int = Const.Materials.DIRT
var selected_floor: int = -1

var cache: Array
var terrain_list: Array
var floor_list: Array
var floor_list2: Array
var first_config_index: int

var terrain_data: Dictionary

func _ready() -> void:
	await owner.ready
	
	terrain_prefab = Prefab.create($\"%TerrainItem\")
	var dir := DirAccess.new()
	var file: String
	
	dir.open(\"res://Resources/Terrain/TerrainTextures\")
	dir.list_dir_begin() # TODOConverter3To4 fill missing arguments https://github.com/godotengine/godot/pull/40547
	file = dir.get_next()
	while not file.is_empty():
		if file in SKIP_WALLS:
			file = dir.get_next()
			continue
		
		if file.get_extension() == \"tres\":
			cache.append(load(dir.get_current_dir().plus_file(file)))
			terrain_list.append(file.get_basename())
		file = dir.get_next()
	
	terrain_group = ButtonGroup.new()
	
	add_material(\"Dirt\", Const.Materials.DIRT)
	add_material(\"Bedrock\", Const.Materials.ROCK)
	add_material(\"Metal\", Const.Materials.WEAK_SCRAP)
	add_material(\"Better Metal\", Const.Materials.STRONG_SCRAP)
	add_material(\"Super Metal\", Const.Materials.ULTRA_SCRAP)
	add_material(\"Lumen\", Const.Materials.LUMEN)
	add_material(\"Eroded Lumen\", Const.Materials.DEAD_LUMEN)
	add_material(\"Foam\", Const.Materials.FOAM)
	add_material(\"Fireproof Foam\", Const.Materials.FOAM2)
	add_material(\"Ashes\", Const.Materials.ASHES)
	add_material(\"Napalm\", Const.Materials.TAR)
	add_material(\"Lava\", Const.Materials.LAVA)
	for i in Const.SwappableMaterials.size():
		add_material(\"\", Const.SwappableMaterials[i], true)
	
	terrain_materials.get_child(0).button.button_pressed = true
	
	dir.open(\"res://Resources/Terrain/FloorTextures\")
	dir.list_dir_begin() # TODOConverter3To4 fill missing arguments https://github.com/godotengine/godot/pull/40547
	file = dir.get_next()
	while not file.is_empty():
		if file in SKIP_FLOORS:
			file = dir.get_next()
			continue
		
		cache.append(load(dir.get_current_dir().plus_file(file)))
		if file.begins_with(\"1\"):
			floor_list.append(file.get_basename().trim_prefix(\"1\"))
		elif file.begins_with(\"2\"):
			floor_list2.append(file.get_basename().trim_prefix(\"2\"))
		file = dir.get_next()
	
	floor_group = ButtonGroup.new()
	
	for i in 4:
		var terrain := terrain_prefab.instantiate()
		terrain.base_id = 50
		surface_floor_textures.add_child(terrain)
		terrain.button.group = floor_group
		terrain.button.connect(\"pressed\", Callable(self, \"on_floor_selected\").bind(i))
		terrain.label.hide()
		terrain.set_configurable()
		
		terrain = terrain_prefab.instantiate()
		terrain.base_id = 60
		base_floor_textures.add_child(terrain)
		terrain.button.group = floor_group
		terrain.button.connect(\"pressed\", Callable(self, \"on_floor_selected\").bind(4 + i))
		terrain.label.hide()
		terrain.set_configurable()

func set_data(data: Dictionary):
	terrain_data = data
	refresh_terrain_data()

func add_material(mat_name: String, mat_id: int, configurable := false):
	var terrain := terrain_prefab.instantiate()
	terrain_materials.add_child(terrain)
	terrain.set_terrain(mat_name, mat_id)
	terrain.button.group = terrain_group
	terrain.button.connect(\"pressed\", Callable(self, \"on_material_selected\").bind(mat_id))
	terrain.set_meta(\"material_id\", mat_id)
	
	if configurable:
		if first_config_index == 0:
			first_config_index = terrain.get_index()
		
		terrain.set_configurable()
		owner.starting_material.add_item(Const.game_data.DEFAULT_TERRAIN_CONFIG.terrain[terrain.get_index() - first_config_index], mat_id)
	else:
		owner.starting_material.add_item(mat_name, mat_id)

func on_material_selected(index: int) -> void:
	get_tree().call_group(\"floor_disable\", \"set_disabled\", false)
	get_tree().call_group(\"floor_disable\", \"set_editable\", true)
	
	selected_material = index
	selected_floor = -1
	if floor_group.get_pressed_button():
		floor_group.get_pressed_button().set_pressed_no_signal(false)
	
	owner.clear_object_settings()
	if index in Const.SwappableMaterials:
		$\"%NiceSeparator\".show()
		
		if not $\"%ObjectSettingsScroll\".visible:
			$\"%MainScroll\".scroll_vertical += $\"%ObjectSettingsScroll\".size.y
		
		var label := Label.new()
		label.text = \"Swap Terrain\"
		label.align = Label.ALIGNMENT_CENTER
		owner.add_object_setting(label)
		
		var flow := HFlowContainer.new()
		flow.add_theme_constant_override(\"h_separation\", 0)
		owner.add_object_setting(flow)
		
		var settings_group := ButtonGroup.new()
		
		var idx := Const.SwappableMaterials.find(index)
		for mat in terrain_list:
			var terrain := terrain_prefab.instantiate()
			terrain.swap = true
			flow.add_child(terrain)
			terrain.set_terrain_mat(mat.capitalize(), load(\"res://Resources/Terrain/TerrainTextures/%s.tres\" % mat))
			terrain.button.group = settings_group
			
			if mat == terrain_data.terrain[idx]:
				terrain.button.button_pressed = true
			
			terrain.button.connect(\"pressed\", Callable(self, \"modify_material\").bind(\"terrain\", idx, mat))

func modify_material(list: String, idx: int, mat):
	terrain_data[list][idx] = mat
	refresh_terrain_data()

func on_floor_selected(index: int):
	get_tree().call_group(\"floor_disable\", \"set_disabled\", true)
	get_tree().call_group(\"floor_disable\", \"set_editable\", false)
	
	selected_floor = index
	selected_material = -1
	if terrain_group.get_pressed_button():
		terrain_group.get_pressed_button().set_pressed_no_signal(false)
	
	var hardness := 1.0
	if is_instance_valid(floor_hardness):
		hardness = floor_hardness.value
	
	owner.clear_object_settings()
	$\"%NiceSeparator\".show()
	if not $\"%ObjectSettingsScroll\".visible:
		Utils.call_super_deferred($\"%MainScroll\", \"set_deferred\", [\"scroll_vertical\", $\"%MainScroll\".scroll_vertical + $\"%ObjectSettingsScroll\".size.y * 100])
	
	var label := Label.new()
	label.text = \"Hardness\"
	label.align = Label.ALIGNMENT_CENTER
	owner.add_object_setting(label)
	
	floor_hardness = HSlider.new()
	floor_hardness.min_value = 0.001
	floor_hardness.max_value = 1
	floor_hardness.step = 0.001
	floor_hardness.value = hardness
	owner.add_object_setting(floor_hardness)
	
	label = Label.new()
	label.text = \"Swap Floor\"
	label.align = Label.ALIGNMENT_CENTER
	owner.add_object_setting(label)
	
	var flow := HFlowContainer.new()
	flow.add_theme_constant_override(\"h_separation\", 0)
	owner.add_object_setting(flow)
	
	var settings_group := ButtonGroup.new()
	
	var idx := index % 4
	var list = floor_list2 if index < 4 else floor_list
	
	for mat in list:
		var terrain := terrain_prefab.instantiate()
		terrain.swap = true
		flow.add_child(terrain)
		terrain.button.group = settings_group
		
		if index < 4:
			terrain.set_floor(load(\"res://Resources/Terrain/FloorTextures/2%s.tres\" % mat), mat.capitalize())
			
			if mat == terrain_data.upper_floor[idx]:
				terrain.button.button_pressed = true
			
			terrain.button.connect(\"pressed\", Callable(self, \"modify_material\").bind(\"upper_floor\", idx, mat))
		else:
			terrain.set_floor(load(\"res://Resources/Terrain/FloorTextures/1%s.tres\" % mat), mat.capitalize())
			
			if mat == terrain_data.lower_floor[idx]:
				terrain.button.button_pressed = true
			
			terrain.button.connect(\"pressed\", Callable(self, \"modify_material\").bind(\"lower_floor\", idx, mat))

func refresh_terrain_data():
	for i in Const.SwappableMaterials.size():
		var mat = load(\"res://Resources/Terrain/TerrainTextures/%s.tres\" % terrain_data.terrain[i])
		terrain_materials.get_child(first_config_index + i).set_terrain_mat(terrain_data.terrain[i].capitalize(), mat)
	
	for i in 4:
		var mat = load(\"res://Resources/Terrain/FloorTextures/2%s.tres\" % terrain_data.upper_floor[i])
		surface_floor_textures.get_child(i).set_floor(mat)
		
		mat = load(\"res://Resources/Terrain/FloorTextures/1%s.tres\" % terrain_data.lower_floor[i])
		base_floor_textures.get_child(i).set_floor(mat)
	
	owner.refresh_terrain_data()

func on_terrain_updated():
	owner.set_unsaved(true)
	owner.refresh_terrain_data()

func pick_material(mat: int):
	for mater in terrain_materials.get_children():
		if mater.get_meta(\"material_id\", -1) == mat:
			mater.button.button_pressed = true
			on_material_selected(mat)
			break
"

