[sub_resource type="GDScript" id=126]
resource_name = "Event"
script/source = "extends Control

@onready var label: Label = $VBoxContainer/VBoxContainer/Title
@onready var condition_container: VBoxContainer = $VBoxContainer/PanelContainer/ConditionContainer
@onready var action_container: VBoxContainer = $VBoxContainer/PanelContainer2/ActionContainer
@onready var any: CheckBox = $VBoxContainer/PanelContainer/ConditionContainer/Any
@onready var add_condition_container: Control = $VBoxContainer/PanelContainer/ConditionContainer/AddCondition
@onready var add_action_container: Control = $VBoxContainer/PanelContainer2/ActionContainer/AddAction
@onready var repeatable: CheckBox = $VBoxContainer/Repeatable

var is_event
var action_prefab: PackedScene
var folded: bool

func _ready() -> void:
	refresh_id()

func refresh_id():
	label.text = tr(\"Event #%s\") % get_index()

func add_condition() -> Object:
	var condition = action_prefab.instantiate()
	condition_container.add_child(condition)
	condition.connect(\"tree_exited\", Callable(self, \"update_repeatable\"))
	update_repeatable()
	add_condition_container.raise()
	return condition

func paste_condition() -> void:
	var data = Utils.get_meta(\"condition_clipboard\", {})
	if not data.is_empty():
		add_condition().set_data(data)

func add_action() -> Object:
	var action = action_prefab.instantiate()
	action.is_action = true
	action_container.add_child(action)
	add_action_container.raise()
	return action

func paste_action() -> void:
	var data = Utils.get_meta(\"action_clipboard\", {})
	if not data.is_empty():
		add_action().set_data(data)

func get_data() -> Dictionary:
	var conditions = []
	for condition in condition_container.get_children():
		if \"is_action\" in condition:
			var data = condition.get_data()
			if not data.is_empty():
				conditions.append(data)
	
	var actions = []
	for action in action_container.get_children():
		if \"is_action\" in action:
			var data = action.get_data()
			if not data.is_empty():
				actions.append(data)
	
	return {conditions = conditions, actions = actions, any = any.pressed, repeatable = repeatable.pressed}

func set_data(data: Dictionary):
	for condition_data in data.conditions:
		var condition = add_condition()
		condition.set_data(condition_data)
	
	for action_data in data.actions:
		var action = add_action()
		action.set_data(action_data)
	
	any.button_pressed = data.any
	repeatable.button_pressed = data.get(\"repeatable\", false)

func delete_event() -> void:
	queue_free()

func copy_event() -> void:
	Utils.set_meta(\"event_clipboard\", get_data())
	get_tree().call_group(\"event_paste\", \"set_disabled\", false)

func toggle_fold(button_pressed: bool) -> void:
	folded = button_pressed
	
	repeatable.visible = not folded
	$VBoxContainer/VBoxContainer/HBoxContainer2.visible = not folded
	$VBoxContainer/PanelContainer.visible = not folded
	$VBoxContainer/PanelContainer2.visible = not folded
	$VBoxContainer/VBoxContainer/Title/Collapse.icon = preload(\"res://Nodes/Editor/GUI/Icons/Expand.svg\") if not button_pressed else preload(\"res://Nodes/Editor/GUI/Icons/Collapse.svg\")

func update_repeatable():
	for condition in condition_container.get_children():
		if \"is_action\" in condition:
			if condition.get_meta(\"instant\", false):
				repeatable.disabled = false
				repeatable.tooltip_text = \"The event can be triggered multiple times.\"
				return
	
	repeatable.disabled = true
	repeatable.button_pressed = false
	repeatable.tooltip_text = \"Requires at least one instant condition.\"
"

