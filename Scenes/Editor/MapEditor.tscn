resource_name = "FileManager"
script/source = "extends PanelContainer

const ROOT = \"user://Maps\"

@onready var goto_parent := find_child(\"GotoParent\")
@onready var files := find_child(\"Files\")
@onready var name_input := find_child(\"NameInput\")
@onready var really_deleter := find_child(\"ReallyDeleter\")

var current_dir := ROOT
var current_file: String
var name_mode: int

func _ready() -> void:
	refresh_dir()

func refresh_dir():
	goto_parent.disabled = current_dir == ROOT
	files.clear()
	
	var dir := DirAccess.new()
	dir.open(current_dir)
	dir.list_dir_begin() # TODOConverter3To4 fill missing arguments https://github.com/godotengine/godot/pull/40547
	
	var dir_list: Array
	var map_list: Array
	
	var file := dir.get_next()
	while not file.is_empty():
		if dir.current_is_dir():
			dir_list.append(file)
		elif file.get_extension() == \"lcmap\":
			map_list.append(file)
		
		file = dir.get_next()
	
	for path in dir_list:
		files.add_item(path)
		files.set_item_icon(files.get_item_count() - 1, preload(\"res://Nodes/Editor/Icons/Folder.svg\"))
		files.set_item_metadata(files.get_item_count() - 1, {directory = current_dir.plus_file(path)})
	
	for path in map_list:
		var map := MapFile.new()
		map.load_metadata(current_dir.plus_file(path))
		if map.map_name.is_empty():
			files.add_item(path)
		else:
			files.add_item(\"%s (%s)\" % [path, map.map_name])
		files.set_item_metadata(files.get_item_count() - 1, {map = current_dir.plus_file(path)})
	
	file_selected(-1)

func _on_Done_pressed() -> void:
	hide()

func file_activated(index: int) -> void:
	var directory = files.get_item_metadata(index).get(\"directory\", \"\")
	if not directory.is_empty():
		current_dir = directory
		refresh_dir()

func file_selected(index: int) -> void:
	if index >= 0:
		var map = files.get_item_metadata(index).get(\"map\", \"\")
		get_tree().call_group(\"rededu\", \"set_disabled\", map.is_empty())
		current_file = map
	
	really_deleter.button_pressed = false
	really_deleter.hide()
	name_input.hide()

func go_to_parent() -> void:
	current_dir = current_dir.get_base_dir()
	refresh_dir()

func try_rename() -> void:
	name_input.text = current_file.get_file().get_basename()
	name_input.placeholder_text = \"New name\"
	name_input.show()
	name_mode = 1

func try_delete() -> void:
	if not really_deleter.pressed:
		really_deleter.show()
	else:
		OS.move_to_trash(ProjectSettings.globalize_path(current_file))
		refresh_dir()

func try_duplicate() -> void:
	name_input.text = current_file.get_file().get_basename()
	name_input.placeholder_text = \"Duplicate name\"
	name_input.show()
	name_mode = 2

func name_entered(new_text: String) -> void:
	if name_mode == 1:
		DirAccess.new().rename(current_file, current_file.get_base_dir().plus_file(new_text) + \".lcmap\")
		refresh_dir()
	elif name_mode == 2:
		DirAccess.new().copy(current_file, current_file.get_base_dir().plus_file(new_text) + \".lcmap\")
		refresh_dir()
"

