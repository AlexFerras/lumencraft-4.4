resource_name = "MyWorkshop"
script/source = "extends Button

var dir := DirAccess.new()

var waiting: int
var loaded: bool

func _pressed() -> void:
	SteamAPI.connect(\"workshop_loaded\", Callable(self, \"on_workshop_loaded\"))
	SteamAPI.connect(\"map_downloaded\", Callable(self, \"on_item_download\"))
	SteamAPI.load_workshop(0, 1, \"\", [])
	text = \"Loading...\"
	disabled = true
	
	get_tree().create_timer(5).connect(\"timeout\", Callable(self, \"check_loaded\"))

func on_workshop_loaded(items: int, c: bool):
	for i in items:
		var data: Dictionary = SteamAPI.singleton.getQueryUGCResult(SteamAPI.workshop_handle, i)
		if data.steam_id_owner == SteamAPI.STEAM_ID:
			SteamAPI.singleton.downloadItem(data.file_id, false)
			waiting += 1
			loaded = true
	
	SteamAPI.release_workshop_handle()

func on_item_download(data: Dictionary):
	dir.make_dir_recursive(\"user://Maps/MyMaps\")
	dir.copy(data.path, str(\"user://Maps/MyMaps/\", data.id, \".lcmap\"))
	
	waiting -= 1
	if waiting == 0:
		get_parent().get_parent().invalidate()
		hide()

func check_loaded():
	if loaded:
		loaded = false
	else:
		disabled = false
"

