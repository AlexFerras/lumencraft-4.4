resource_name = "Music Player"
script/source = "extends PanelContainer

@onready var audio_player: AudioStreamPlayer = $AudioStreamPlayer
@onready var music_list: ItemList = $VBoxContainer/HBoxContainer/ItemList
@onready var playing: BaseButton = $VBoxContainer/HBoxContainer/VBoxContainer/Button
@onready var single: BaseButton = $VBoxContainer/HBoxContainer/VBoxContainer/CheckBox

const SONG_TITLES = {
	\"heroic\": \"Epic Symphonic Dubstep\"
}

func _ready() -> void:
	for music in Music.get_music_list():
		add_track(music)
	add_track(Music.Campaign.hub1)
	add_track(Music.Campaign.hub2)
	add_track(Music.Campaign.hub3)
	add_track(Music.Campaign.endgame)
	
	music_list.select(0)
	hide()

func add_track(path: String):
	var track_name := path.get_file().get_basename()
	music_list.add_item(SONG_TITLES.get(track_name, track_name))
	music_list.set_item_metadata(music_list.get_item_count() - 1, path)

func play_toggle(button_pressed: bool) -> void:
	if button_pressed:
		audio_player.stream = load(music_list.get_item_metadata(music_list.get_selected_items()[0]))
		audio_player.play()
	else:
		audio_player.stop()

func music_selected(index: int) -> void:
	playing.set_pressed_no_signal(true)
	play_toggle(true)

func next_track() -> void:
	if not playing.pressed:
		return
	
	if single.pressed:
		audio_player.play()
	else:
		music_list.select((music_list.get_selected_items()[0] + 1) % music_list.get_item_count())
		play_toggle(true)
"

