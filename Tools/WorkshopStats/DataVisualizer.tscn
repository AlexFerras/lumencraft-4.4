[sub_resource type="GDScript" id=2]
script/source = "extends Control

var all_data: Dictionary

func _ready() -> void:
	var file := File.new()
	file.open(\"res://Tools/WorkshopStats/LastData.dat\", file.READ)
	all_data = file.get_var()
	
	var keys = all_data.keys()
	keys.sort()
	
	for data in keys:
		var graph = preload(\"res://Tools/WorkshopStats/DataGraph.tscn\").instantiate()
		$VBoxContainer.add_child(graph)
		graph.set_data(data, all_data[data])
"

