script/source = "extends VBoxContainer

static var RESOLUTIONS = [
#	Vector2(1024, 768), # 4:3
	Vector2(1280, 720), # 16:9 
	Vector2(1280, 800), # 16:10   X
	Vector2(1366, 768), # 16:9    X
	Vector2(1440, 900), # 16:10   X
	Vector2(1920, 1080), # 16:9   X
	Vector2(1920, 1200), # 16:10
	Vector2(1920, 1440), # 4:3    X
	Vector2(2560, 1080), # 21:9
	Vector2(2560, 1440), # 16:9   X
	Vector2(2560, 1600), # 16:10
	Vector2(3440, 1440), # 21:9
	Vector2(3840, 2160), # 16:9   X
]

static var FRAME_CAP = [0, 30, 45, 59, 60, 72, 90, 120, 144, 165]

const LANGUAGE_LIST = [\"en\", \"pl\", \"fr\", \"es\", \"de\", \"ja\", \"zh\", \"ko\", \"uk\", \"ru\", \"da\", \"id\", \"hu\", \"it\", \"nl\", \"pt_BR\", \"pt_PT\", \"ro\", \"sk\" ,\"sv\", \"tr\",
\"es\", \"cs\", \"el\", \"vi\", \"bg\", \"fi\", \"nb\"]

var sliders_list := []
var buttons_list := []
var toggles_list := []
var groups_list  := []
var resolutions_group_list  := []
var previous_focus: Control

var confirm_setting: String
var confirm_previous

func _ready() -> void:
	set_process(is_visible_in_tree())
	
	if Music.is_switch_build():
		$\"%Display\".queue_free()
		$\"%Singlep\".queue_free()
		$\"%Relat\".queue_free()
		$\"%MuteWhenUnfocused\".hide()
	
	if not DisplayServer.screen_get_size() in RESOLUTIONS:
		RESOLUTIONS.append(DisplayServer.screen_get_size())
	
	# wiem Kobe. Wszystko do wyjebania.
	var base_button = $\"%Resolution\"
	for index in RESOLUTIONS.size() - 1:
		var new_resolution_button = base_button.duplicate()
		new_resolution_button.text = stringify_resolution(RESOLUTIONS[index+1])
		$\"%Resolutions\".add_child(new_resolution_button)
	# --------------------------------
		
	
	if not DisplayServer.screen_get_refresh_rate() in FRAME_CAP:
		FRAME_CAP.append(DisplayServer.screen_get_refresh_rate())
	# TODO
	#bind_group_option($\"%ScreenMode\".group, \"screenmode\")
	#if Save.config.screenmode == Config.WINDOWED:
		#bind_resolution_group_option($\"%Resolution\".group, \"windowed_resolution\")
	#else:
		#bind_resolution_group_option($\"%Resolution\".group, \"fullscreen_resolution\")
		
	update_resolutions()
	bind_toggle_option($\"%VSync\", \"vsync\")
	bind_list_option($\"%LimitFPS\", \"limit_fps\", FRAME_CAP, PackedStringArray(Utils.array_map(FRAME_CAP, Callable(self, \"stringify_frame\"))))
	bind_slider_option($\"%ViewportSize\", \"light_downsample\", 1, 5, 1, \"light_downsample\")
	bind_slider_option($\"%Raytrace\", \"shadow_render_steps\", 2, 10)#, 1, \"shadow_render_steps\")
	bind_toggle_option($\"%HighQualityGlow\", \"glow_high_quality\")
	bind_list_option($\"%GlowType\", \"glow_type\", [1, 2], [\"1\", \"2\"])
	bind_slider_option($\"%GlowIntensity\", \"glow_intensity\", 0, 2, 0.05, \"intensity\")
	
	bind_slider_toggle_option($\"%SoundVolume\", \"sound_volume\", \"sound_enabled\", 0, 1, 0.01, \"audio\")
	bind_slider_toggle_option($\"%MusicVolume\", \"music_volume\", \"music_enabled\", 0, 1, 0.01, \"audio\")
	bind_toggle_option($\"%MuteUnfocused\", \"mute_when_unfocused\")
	
	bind_list_option($\"%Language\", \"language\", LANGUAGE_LIST, PackedStringArray(Utils.array_map(LANGUAGE_LIST, Callable(self, \"english\"))))
	bind_slider_option($\"%UIScale\", \"ui_scale\", 0.7, 1, 0.01, \"percent\")
	bind_toggle_option($\"%AutoSave\", \"enable_autosave\")
	bind_toggle_option($\"%HealthBars\", \"show_enemy_health\")
	bind_toggle_option($\"%DamageNumbers\", \"show_damage_numbers\")
	bind_toggle_option($\"%LowHealthAlarm\", \"low_health_alarm\")
	bind_toggle_option($\"%AlternateMovement\", \"alternate_movement\")
	bind_toggle_option($\"%ControlTooltips\", \"show_control_tooltips\")
	#TODO
	#bind_group_option($\"%SPControls\".group, \"single_player_controls\")
	
	bind_slider_option($\"%Vibrations\", \"joypad_vibrations\", 0, 1, 0.05)
	bind_slider_option($\"%ScreenShake\", \"screenshake\", 0, 1, 0.05)
	bind_slider_option($\"%CursorScale\", \"cursor_scale\", 0, 2, 0.01)
	bind_toggle_option($\"%DoubleZoom\", \"additional_zoom\")
	bind_toggle_option($\"%AimHack\", \"aim_hack\")

func bind_toggle_option(button: Button, option: String):
	button.button_pressed = Save.config.get(option)
	button.connect(\"toggled\", Callable(self, \"set_setting\").bind(option))
	toggles_list.append([button, option])

func bind_group_option(group: ButtonGroup, option: String):
	var container = group.get_buttons().front().get_parent()
	assert(container.get_child_count() == group.get_buttons().size())
	container.get_child(Save.config.get(option)).button_pressed = true
	group.connect(\"pressed\", Callable(self, \"handle_group\").bind(option))
	groups_list.append([container, option])

func bind_resolution_group_option(group: ButtonGroup, option: String):
	var container = group.get_buttons().front().get_parent()
	assert(container.get_child_count() == group.get_buttons().size())
	var i = RESOLUTIONS.find(Save.config.get(option))
	if i > -1:
		container.get_child( i ).button_pressed = true
	group.connect(\"pressed\", Callable(self, \"handle_resolution_group\").bind(option))
	resolutions_group_list.append([container, option])

func bind_list_option(button: Button, option: String, options: Array, strings: PackedStringArray):
	button.strings = strings
	button.options = options
	button.current_option = options.find(Save.config.get(option))
	button.connect(\"option_selected\", Callable(self, \"set_setting\").bind(option))
	buttons_list.append([button, options, option])

func bind_slider_option(slider: ProgressBar, option: String, minimum: float, maximum: float, step: float = 1, display_func := \"\"):
	slider.min_value = minimum
	slider.max_value = maximum
	slider.step = step
	slider.display_func = display_func
	slider.value = randf() # tak trzeba. Nie pytaj
	slider.value = Save.config.get(option)
	slider.connect(\"value_changed\", Callable(self, \"set_setting\").bind(option))
	sliders_list.append([slider, option])

func bind_slider_toggle_option(slider: ProgressBar, option: String, toggle: String, minimum: float, maximum: float, step: float = 1, display_func := \"\"):
	## chyba TODO
	bind_slider_option(slider, option, minimum, maximum, step, display_func)

func set_setting(value, setting: String):
	confirm_previous = Save.config.get(setting)
	
	Save.config.set(setting, value)
	
	if need_confirm(setting):
		confirm_setting = setting
		previous_focus = get_viewport().gui_get_focus_owner()
		$\"%ConfirmOptions\".get_parent().show()
		$\"%ConfirmOptions\".showme()
		$\"%ConfirmOptions/%ButtonCancel\".grab_focus()

	match setting:
		\"light_downsample\":
			Save.config.apply_downsampling()
			pass
		\"shadow_render_steps\":
			Save.config.apply_shadow_steps()
			pass
		\"fullscreen_resolution\":
			request_viewport_resize()
		\"windowed_resolution\":
			Save.config.request_refresh_display()
			request_viewport_resize()
		\"screenmode\":
			Save.config.request_refresh_display()
			update_resolutions()
			request_viewport_resize(1.2)
	Save.config.apply()

func handle_group(button: Button, option: String):
	set_setting(button.get_index(), option)

func handle_resolution_group(button: Button, option: String):
	set_setting(RESOLUTIONS[button.get_index()], option)

func stringify_resolution(res: Vector2) -> String:
	return str(res.x, \" x \", res.y)

func stringify_frame(f: int) -> String:
	return \"OFF\" if f == 0 else str(f)

func english(lang: String) -> String:
	return \"English\"

var requested_viewport_resize := 0.5
func request_viewport_resize(time := 0.5):
	set_physics_process(true)
	requested_viewport_resize = time

func _physics_process(delta):
	if requested_viewport_resize > 0:
		requested_viewport_resize -= delta
		if requested_viewport_resize <= 0:
			Save.config.apply_vieport_size()
			set_physics_process(false)

func update_resolutions():
	resolutions_group_list = []
	# TODO
	#if $\"%Resolution\".group.is_connected(\"pressed\", Callable(self, \"handle_resolution_group\")):
	#	$\"%Resolution\".group.disconnect(\"pressed\", Callable(self, \"handle_resolution_group\"))

	#if Save.config.screenmode == Config.WINDOWED:
		#$\"%ResolutionName\".text = \"Window Resolution\"
		#bind_resolution_group_option($\"%Resolution\".group, \"windowed_resolution\")
	#else:
		#$\"%ResolutionName\".text = \"Fullscreen Resolution\"
		#bind_resolution_group_option($\"%Resolution\".group, \"fullscreen_resolution\")

	refresh_selected_resolutions()

func connect_close(object: Object, method: String):
	$\"%ExitButton\".connect(\"pressed\", Callable(object, method))

func get_first_control():
	if has_node(\"%ScreenMode\"):
		return $\"%ScreenMode\"
	else:
		return $\"%SoundVolume\"

func change_controls() -> void:
	$\"%MainOptions\".hide()
	$\"%ControlOptionsParent\".show()
	$\"%ControlOptions\".get_first_control().call_deferred(\"grab_focus\")

func close_controls():
	$\"%ControlOptionsParent\".hide()
	$\"%MainOptions\".show()
	get_first_control().call_deferred(\"grab_focus\")

func reset_options():
	Save.reset_config()
	refresh_selected_options()

func refresh_selected_options():
	for toggle_option in toggles_list:
		toggle_option[0].button_pressed = Save.config.get(toggle_option[1])
	
	for group_option in groups_list:
		group_option[0].get_child(Save.config.get(group_option[1])).button_pressed = true
	
	refresh_selected_resolutions()
	
	for button_option in buttons_list:
		button_option[0].current_option = button_option[1].find(Save.config.get(button_option[2]))
	
	for slider_option in sliders_list:
		slider_option[0].value = Save.config.get(slider_option[1])

func refresh_selected_resolutions():
	for group_option in resolutions_group_list:
		var i = RESOLUTIONS.find(Save.config.get(group_option[1]))
		if i > -1:
			group_option[0].get_child(i).button_pressed = true

func need_confirm(setting: String) -> bool:
	return setting in [\"screenmode\", \"windowed_resolution\", \"fullscreen_resolution\"]

func on_change_confirm() -> void:
	previous_focus.grab_focus()
	previous_focus = null
	$\"%ConfirmOptions\".hideme()

func on_change_cancel() -> void:
	if not previous_focus or $\"%ConfirmOptions\".is_visible_in_tree():
		return
	
	previous_focus.grab_focus()
	$\"%ConfirmOptions\".hideme()
	Save.config.set(confirm_setting, confirm_previous)
	if confirm_setting == \"screenmode\" or confirm_setting == \"windowed_resolution\":
		Save.config.request_refresh_display()
	Save.config.apply()
	refresh_selected_options()

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		set_process(is_visible_in_tree())

func _process(delta: float) -> void:
	var tabs = $\"%TabContainer\"
	if Input.is_action_just_pressed(\"next_tab\"):
		tabs.current_tab = posmod(tabs.current_tab + 1, tabs.get_tab_count())
		tabs.get_tab_control(tabs.current_tab).get_child(0).find_next_valid_focus().call_deferred(\"grab_focus\")
	elif Input.is_action_just_pressed(\"prev_tab\"):
		tabs.current_tab = posmod(tabs.current_tab - 1, tabs.get_tab_count())
		tabs.get_tab_control(tabs.current_tab).get_child(0).find_next_valid_focus().call_deferred(\"grab_focus\")
"

