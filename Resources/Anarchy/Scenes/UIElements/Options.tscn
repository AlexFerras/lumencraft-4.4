resource_name = "Confirm"
script/source = "extends \"res://Resources/Anarchy/Scripts/warning_window.gd\"

@onready var message = $\"%Message\".text

var timer: float

func _ready() -> void:
	set_physics_process(false)

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		set_physics_process(is_visible_in_tree())
		timer = 9

func _physics_process(delta: float) -> void:
	timer -= delta
	$\"%Message\".text = tr(message) % ceil(timer)
	
	if timer <= 0:
		hide()
"

