resource_name = "MainMenu"
script/source = "extends Node

const DIFFICULTY_DESCRIPTIONS = [
	\"For beginner scouts.\\n• 50% bonus resources from digging\\n• 1000 starting resources\",
	\"For seasoned diggers.\\n• 10% bonus resources from digging\\n• 300 starting resources\",
	\"For advanced drillers.\\n• No bonus resources from digging\\n• No starting resources\",
	\"For veteran delvers.\\n• 30% less resources from digging\\n• No starting resources\",
]

@onready var main_panel := $\"%MainMenuLeftPanel\"
@onready var changelog := $\"%ChangeLog\"
@onready var sub_panels := [$\"%PlayGameMenu\", $\"%MapDetails\", $\"%SteamWorkshop\", $\"%LoadGame\", $\"%VersionWarning\", $\"%Options\", $\"%Leaderboards\", $\"%ExtrasMenu\", $\"%Hints\", $\"%Credits\", $\"%Secret\", $\"%Generation\"]
@onready var options_control := $\"%Options\"

var current_map_description
var closator: Tween

var method_to_call: MethodCall

func _ready() -> void:
	get_tree().auto_accept_quit = true
	get_tree().paused = false
	
	hide_sub_panels()
	options_control.connect_close(self, \"hide_options\")
	
	if SteamAPI.active:
		if not SteamAPI.initialized or Music.is_demo_build():
			$\"%BrowseWorkshop\".disabled = true
			$\"%LeaderboardsButton\".get_child(0).disabled = true
		
		if Music.is_demo_build():
			$\"%LocalContainer\".hide()
		
		SteamAPI.connect(\"download_started\", Callable(self, \"on_start_download\"))
		SteamAPI.connect(\"map_downloaded\", Callable($\"%PlayGameMenu\", \"on_map_downloaded\"))
		SteamAPI.connect(\"map_unsubscribed\", Callable($\"%PlayGameMenu\", \"on_map_unsubscribed\"))
		
		if SteamAPI.downloading_maps:
			on_start_download()
	else:
		$\"%WorkshopContainer\".hide()
		$\"%LeaderboardsButton\".hide()
	
	if not $\"%LoadGame\".is_any_save:
		$\"%LoadButton\".get_child(0).disabled = true
		
	Music.play_music(Music.TITLE)
	
	$\"%InputHell\".hide()
	$\"%ConfirmQuit\".hide()
	$\"%DifficultyPopup\".hide()
	$\"%CoopTooltip\".hide()
	
	# TODO
	#var group: ButtonGroup = $\"%Difficulty1\".group
	#group.connect(\"pressed\", Callable(self, \"difficulty_selected\"))
	#for button in group.get_buttons():
		#button.connect(\"mouse_entered\", Callable(self, \"set_difficulty_description\").bind(button.name.to_int()))
		#button.connect(\"focus_entered\", Callable(self, \"set_difficulty_description\").bind(button.name.to_int()))
	
	var date = Time.get_date_dict_from_system()
	if (date.month == 12 and date.day >= 15) or (date.month == 1 and date.day <= 15):
		$Snow.propagate_call(\"set_emitting\", [true])
	
	if $\"%LoadGame\".campaign_slots.is_empty():
		$\"%ContinueCampaign\".disabled = true
	
	if OS.has_feature(\"mobile\"):
		$\"%MapEditor\".hide()
		$\"%Scorecard\".hide()
		$\"%Maxp\".hide()
		$\"%LocalContainer\".hide()
	
	if Music.is_switch_build():
		$\"%FearMap\".hide()
		$\"%CRT\".hide()
		$\"%3DStuff\".queue_free()
		$\"%Quit\".hide()
		$\"%Discord\".hide()
		$\"%Twitter\".hide()
		$\"%Social\".modulate.a = 0
		$\"%GenMapSize\".max_value = 3328
		$\"%Feedback\".hide()
		changelog.hide()
	else:
		$\"%VideoStreamPlayer\".play()
		$\"%1Frame\".hide()
	
	if Music.is_switch_build() or Const.get_override(\"TEST_PREGENERATED_MAPS\"):
		$PregeneratedMaps.prepare()

func hide_sub_panels():
	for panel in sub_panels:
		panel.hide()
	
	if not OS.has_feature(\"mobile\"):
		changelog.show()
	$\"%Play\".get_child(0).call_deferred(\"grab_focus\")
	$\"%Social\".show()

func hide_options():
	if options_control.get_node(\"%ControlOptionsParent\").visible:
		options_control.close_controls()
	else:
		hide_sub_panels()

func show_menu(menu: CanvasItem):
	hide_sub_panels()
	changelog.hide()
	menu.show()
#	$\"%CoopTooltip\".visible = (menu == $\"%PlayGameMenu\")
	$\"%Social\".hide()

func play_game() -> void:
	show_menu($\"%PlayGameMenu\")
	if $PregeneratedMaps.active:
		$\"%Randomedium\".call_deferred(\"grab_focus\")
	else:
		$\"%RandomStart\".call_deferred(\"grab_focus\")

func start_game(map, ranked := false, confirmed := false, confirmed_tutorial := false):
	if confirmed:
		confirmed_tutorial = true
	
	if not confirmed_tutorial and not is_tutorial_complete():
		show_tutorial_prompt(self, \"start_game\", [map, ranked, false, true])
		return
	
	if not confirmed and map.get_file().get_basename() in Const.BASE_DEFENSE_MAPS:
		show_difficulty_prompt(self, \"start_game\", [map, ranked, true, true])
		return
	
	if is_queued_for_deletion():
		return
	
	Save.new_game()
	
	var loading = preload(\"res://Scenes/ScreenLoading.tscn\").instantiate()
	Utils.add_child(loading)
	await RenderingServer.frame_post_draw
	loading.queue_free()
	
	prepare_loading()
#	Save.sandbox_options = sandbox.get_options()
	Game.start_map(map)
	Save.data.ranked = ranked
	Save.data.achievements_enabled = ranked or map is MapFile
	
	if map is MapFile and OS.has_feature(\"editor\"):
		map.save_to_file(\"user://gen.lcmap\")

func load_game() -> void:
	show_menu($\"%LoadGame\")
	$\"%LoadGame\".list.get_child(1).call_deferred(\"grab_focus\")

func tutorial() -> void:
	start_game(\"res://Maps/TutorialMap.tscn\", false, true)

func options() -> void:
	show_menu($\"%Options\")
	$\"%Options\".get_node(\"%TabContainer\").current_tab = 0
	$\"%Options\".close_controls()
	$\"%Options\".get_first_control().call_deferred(\"grab_focus\")

func browse_workshop() -> void:
	show_menu($\"%SteamWorkshop\")

func leaderboards() -> void:
	show_menu($\"%Leaderboards\")

func map_editor() -> void:
	Utils.change_scene_with_loading(\"res://Scenes/Editor/MapEditor.tscn\")

func quit() -> void:
	get_tree().quit()

func show_map_description(for_map):
	current_map_description = for_map
	if is_instance_valid(closator):
		closator.kill()
		closator = null
	
	if not for_map.has_meta(\"description\"):
		$\"%MapDetails\".hide()
		return
	
	$\"%MapDetails\".show()
	$\"%MapDescription\".text = for_map.get_meta(\"description\")
	
	$\"%Difficulty\".set_meta(\"difficulty\", for_map.get_meta(\"difficulty\", 0))
	update_difficulty()

func update_difficulty():
	var difficulty: int = $\"%Difficulty\".get_meta(\"difficulty\", 0)
	if difficulty > 0:
		$\"%Difficulty\".show()
		for skull in $\"%Difficulty\".get_children():
			if difficulty > skull.get_index():
				skull.self_modulate = Const.UI_MAIN_COLOR
			else:
				skull.self_modulate = Color.WHITE
	else:
		$\"%Difficulty\".hide()

func hide_map_description(for_map):
	if current_map_description == for_map:
		closator = create_tween()
		closator.tween_interval(0.2)
		closator.tween_callback(Callable($\"%MapDetails\", \"hide\"))
		current_map_description = null

func _input(event: InputEvent) -> void:
	if $\"%Options\".visible:
		return
	
	if event is InputEventKey and event.pressed and event.keycode == KEY_F1:
		Utils.open_folder(ProjectSettings.globalize_path(\"user://logs\"))
	
	if OS.has_feature(\"expo\") and event is InputEventKey and event.pressed and event.keycode == KEY_F5:
		get_tree().change_scene_to_file(\"res://Scenes/TrailerPlayer.tscn\")
	
	if Music.is_game_build() and not OS.has_feature(\"dev_build\"):
		return
	
	if event is InputEventKey and event.pressed and event.keycode == KEY_F3:
		if event.shift:
			start_game(\"res://Maps/TestMapMinimal.tscn\")
		elif event.control:
			start_game(\"res://Maps/TestMapPromo1000.tscn\")
		else:
			start_game(\"res://Maps/TestMap.tscn\")
		
	if event is InputEventKey and event.pressed and event.keycode == KEY_F4:
		if event.shift and event.control:
#			Utils.set_meta(\"start_override\", Vector2(250, 230))
			start_game(\"res://Maps/Campaign/Hub.tscn\", false, false)
			Save.campaign = CampaignData.new()
			Save.campaign.start()
			Save.campaign.unlock_all = true
			Save.campaign.first = false
		elif event.shift:
			start_editor()
		elif event.control:
			start_random_map2()
			set_process_input(false)
		else:
			start_random_map()
			set_process_input(false)
	
	if event is InputEventKey and event.pressed and event.keycode == KEY_F6:
		SteamAPI.singleton.resetAllStats(true)

	if event is InputEventKey and event.keycode == KEY_L and event.command:
		if event.shift:
			Save.set_meta(\"force_map\", \"report\")
			Save.load_game(\"Report\")
			prepare_loading()
		else:
			Save.current_map = \"\"
			print(\"loading...\")
			Save.load_game(\"debug\")
			prepare_loading()

func start_editor():
	Utils.set_meta(\"editor_debug\", \"user://temp.lcmap\")
	Utils.change_scene_with_loading(\"res://Scenes/Editor/MapEditor.tscn\")

func start_random_map(confirmed := false):
	var time = Time.get_ticks_msec()
	var generator = load(\"res://Nodes/Map/Generator/ChunkedMapGenerator.gd\").new()
	generator.generate()
	
	await generator.map_generated
	prints(\"Generation time:\", Time.get_ticks_msec() - time)
	
	start_game(generator.map_file)

func random_map(confirmed := false):
	if not confirmed and not is_tutorial_complete():
		show_tutorial_prompt(self, \"random_map\", [true])
		return
	
	if $PregeneratedMaps.active:
		start_game($PregeneratedMaps.get_map(), false, true)
		return
	
	show_menu($\"%Generation\")
	$\"%Generate\".call_deferred(\"grab_focus\")

func random_map_with_difficulty(difficulty: int, confirmed := false) -> void:
	if not confirmed and not is_tutorial_complete():
		show_tutorial_prompt(self, \"random_map_with_difficulty\", [difficulty, true])
		return
	
	random_map(true)
	Save.data.difficulty = difficulty

func start_random_map2():
	hide_sub_panels()
	$\"%GeneratingPanel\".gen()
	$\"%MainMenu\".hide()
	
	await get_tree().idle_frame
	
	var time = Time.get_ticks_msec()
	var generator = load(\"res://Nodes/Map/Generator/GeneratedMapBase.tscn\").instantiate()
	
	generator.objective = $\"%GenObjective\".get_option()
	generator.map_size = $\"%GenMapSize\".value
	generator.map_biome = $\"%GenBiome\".get_option()
	generator.wave_count = $\"%GenWaveCount\".value
	generator.enemy_multiplier = $\"%GenEnemies\".value
	generator.wave_multiplier = $\"%GenWavePower\".value
	generator.wave_time_multiplier = $\"%GenWaveTime\".value
	generator.item_multiplier = $\"%GenTreasures\".value
	generator.objective_difficulty = $\"%GenObjectiveDifficullty\".value * 0.05
	generator.starting_resources = $\"%GenStartingResources\".value
	generator.resource_rate = $\"%GenResourceRate\".value
	generator.endless = $\"%Endless\".pressed
	
	var seed_to_set: String = $\"%GenSeed\".text
	if seed_to_set.is_empty():
		pass
	elif seed_to_set.find(\"/\") > -1:
		var chunk := seed_to_set.get_slice(\"/\", 0)
		generator.map_size = chunk.substr(0, 4).to_int()
		generator.wave_count = chunk.substr(4).to_int()
		
		chunk = seed_to_set.get_slice(\"/\", 1)
		var item := 0
		generator.objective = chunk.substr(item, 2).to_int() - 1
		item += 2
		generator.enemy_multiplier = chunk.substr(item, 2).to_float() * 0.1
		item += 2
		generator.wave_multiplier = chunk.substr(item, 2).to_float() * 0.1
		item += 2
		generator.wave_time_multiplier = chunk.substr(item, 2).to_float() * 0.1
		item += 2
		generator.item_multiplier = chunk.substr(item, 2).to_float() * 0.1
		item += 2
		generator.objective_difficulty = chunk.substr(item, 3).to_float() * 0.05
		item += 3
		generator.resource_rate = chunk.substr(item, 2).to_float() * 0.1
		item += 2
		generator.map_biome = chunk.substr(item, 2).to_int() - 1
		item += 2
		generator.starting_resources = chunk.substr(item, 4).to_int()
		
		generator.forced_seed = seed_to_set.get_slice(\"/\", 2).to_int()
	elif seed_to_set.is_valid_int():
		generator.forced_seed = int(seed_to_set)
	else:
		generator.forced_seed = seed_to_set.hash()
	
	generator.connect(\"status\", Callable($\"%GeneratingPanel\", \"set_status\"))
	Utils.add_child(generator)
	
	await generator.finished
	prints(\"Generation time:\", Time.get_ticks_msec() - time)
	
	$\"%GeneratingPanel\".set_status(\"Saving map\")
	await get_tree().idle_frame
	
	var mf = generator.map_file
	generator.queue_free()
	
	$\"%GeneratingPanel\".set_status(\"Starting\")
	await get_tree().idle_frame
	
	start_game(mf, false, true)
#	Utils.set_meta(\"editor_debug\", \"user://random.lcmap\")
#	Utils.change_scene_with_loading(\"res://Scenes/Editor/MapEditor.tscn\")

func on_start_download():
	$\"%WorkshopDownload\".need_download(SteamAPI.downloading_maps)

func report_bug() -> void:
	var report: Control = preload(\"res://Resources/Anarchy/Scenes/UIElements/bug_reporting_tool.tscn\").instantiate()
	report.connect(\"tree_exited\", Callable($\"%CRT\", \"set_mouse_filter\").bind(Control.MOUSE_FILTER_IGNORE))
	$AnarchyUI.add_child(report)
	report.get_node(\"%Title\").call_deferred(\"grab_focus\")
	$\"%CRT\".mouse_filter = Control.MOUSE_FILTER_STOP

func twitter() -> void:
	OS.shell_open(\"https://twitter.com/Lumencraft_game\")

func discord() -> void:
	OS.shell_open(\"https://discord.gg/PCeTvgy4nZ\")

func confirm_start_game_with_hide():
	$\"%ConfirmQuit\".hideme()
	await $\"%ConfirmQuit\".was_hidden
	var call := method_to_call
	method_to_call = null
	call.execute()

func confirm_start_game() -> void:
	var call := method_to_call
	method_to_call = null
	call.execute()

func skip_tutorial() -> void:
	Save.player_data.add_completed_map(\"res://Maps/TutorialMap.tscn\")
	confirm_start_game_with_hide()

func extras() -> void:
	show_menu($\"%ExtrasMenu\")
	$\"%HintsButton\".call_deferred(\"grab_focus\")

func open_scorecards() -> void:
	DirAccess.make_dir_recursive_absolute(\"user://scorecards\")
	Utils.open_folder(ProjectSettings.globalize_path(\"user://scorecards\"))

func hints() -> void:
	show_menu($\"%ExtrasMenu\")
	$\"%Hints\".show()
	$\"%HintsButton\".call_deferred(\"grab_focus\")

func credits() -> void:
	show_menu($\"%ExtrasMenu\")
	$\"%Credits\".show()

func secret() -> void:
	show_menu($\"%ExtrasMenu\")
	$\"%Secret\".show()
	if Music.is_switch_build():
		$\"%EnableSplitScreen\".call_deferred(\"grab_focus\")
	else:
		$\"%MaxPlayerCount\".call_deferred(\"grab_focus\")

func continue_campaign() -> void:
	var latest_slot: String = $\"%LoadGame\".campaign_slots.front()
	var latest_time := FileAccess.get_modified_time(Save.get_save_dir(latest_slot).path_join(\"data.tres\"))
	
	for slot in $\"%LoadGame\".campaign_slots:
		var time := FileAccess.get_modified_time(Save.get_save_dir(slot).path_join(\"data.tres\"))
		if time > latest_time:
			latest_slot = slot
			latest_time = time
	
	var loading = preload(\"res://Scenes/ScreenLoading.tscn\").instantiate()
	Utils.add_child(loading)
	await RenderingServer.frame_post_draw
	loading.queue_free()
	
	Save.load_game(latest_slot)
	prepare_loading()

func start_campaign(confirmed := false) -> void:
	if not confirmed and not is_tutorial_complete():
		show_tutorial_prompt(self, \"start_campaign\", [true])
		return
	
	start_game(\"res://Maps/Campaign/Hub.tscn\", false, true)
	Save.campaign = CampaignData.new()
	Save.campaign.start()

func is_tutorial_complete() -> bool:
	return Save.player_data.is_map_completed(\"res://Maps/TutorialMap.tscn\")

func _notification(what: int) -> void:
	if what == Control.NOTIFICATION_THEME_CHANGED:
		update_difficulty()

func difficulty_selected(diff):
	confirm_start_game()
	Save.data.difficulty = diff.name.to_int()

func set_difficulty_description(idx: int):
	$\"%DifficultyDescription\".text = DIFFICULTY_DESCRIPTIONS[idx]

func play_tundra() -> void:
	start_game(\"res://Maps/TundraEscavation.lcmap\", false, true)

func prepare_loading():
	if not Music.is_switch_build():
		$\"%3DStuff\".show()
		Utils.set_meta(\"loading_background\", $\"%3DStuff\")
	queue_free()

func show_tutorial_prompt(instance: Object, method: String, args: Array):
	method_to_call = MethodCall.new()
	method_to_call.instance = instance
	method_to_call.method = method
	method_to_call.args = args
	
	$\"%InputHell\".show()
	$\"%ConfirmQuit\".showme()
	$\"%ConfirmQuit/%ButtonCancel\".call_deferred(\"grab_focus\")

func show_difficulty_prompt(instance: Object, method: String, args: Array):
	method_to_call = MethodCall.new()
	method_to_call.instance = instance
	method_to_call.method = method
	method_to_call.args = args
	
	$\"%InputHell\".show()
	$\"%DifficultyPopup\".showme()
	$\"%Difficulty1\".call_deferred(\"grab_focus\")

class MethodCall:
	var instance: Object
	var method: String
	var args: Array
	
	func execute():
		instance.callv(method, args)

func dwarf() -> void:
	OS.shell_open(\"https://store.steampowered.com/app/1714900?utm_source=Lumen_button\")
"

