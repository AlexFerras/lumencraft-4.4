resource_name = "Changelog"
script/source = "extends RichTextLabel

func _ready() -> void:
	redraw()

func redraw():
	var f = FileAccess.open(\"res://changelog.md\", FileAccess.READ)
	
	var first_line: int
	var versions: int
	
	var notes = Array(f.get_as_text().split(\"\\n\"))
	for i in notes.size():
		if notes[i].find(\"VERSION\") > -1:
			if first_line == 0:
				first_line = i
			
			versions += 1
			if versions == 4:
				notes = notes.slice(first_line, i - 2)
				break
	
	var version_color = Const.UI_MAIN_COLOR.to_html(false)
	var section_color = Const.UI_MAIN_COLOR.lightened(0.5).to_html(false)
	
	for i in notes.size():
		var line: String = notes[i]
		if line.begins_with(\"## \"):
			notes[i] = \"[color=#%s]%s[/color]\" % [version_color, line]
		if line.begins_with(\"###\"):
			notes[i] = \"[color=#%s]%s[/color]\" % [section_color, line]
	
	text = \"\\n\".join(PackedStringArray(notes))
	
	set_process(is_visible_in_tree())

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		set_process(is_visible_in_tree())
	elif what == NOTIFICATION_THEME_CHANGED:
		if is_visible_in_tree():
			clear()
			redraw()

func _process(delta: float) -> void:
	if Input.is_action_pressed(\"p1_look_up\"):
		get_v_scroll_bar().value -= 500 * delta
	elif Input.is_action_pressed(\"p1_look_down\"):
		get_v_scroll_bar().value += 500 * delta
"

