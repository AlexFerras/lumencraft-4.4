resource_name = "Secret"
script/source = "extends Label

var expnded = false
var max_notes_size = 0.0
var max_content_size = 0.0
@onready var notes = $\"../../../Content/Notes\"
@onready var content = $\"../../../Content\"
@onready var ultimate_tooltip = $\"../../../UltimateTooltip\"

func _ready():
#	max_notes_size = notes.rect_size.y
	max_content_size = content.size.y
	content.size_flags_vertical = 1
	content.add_theme_stylebox_override(\"panel\", content.stylebox)
	ultimate_tooltip.hide()
	content.size.y = 0.0
#	notes.rect_size.y = 0.0
#	yield(get_tree(), \"idle_frame\")
	
	
func _gui_input(event: InputEvent) -> void:
	if event is InputEventMouseButton:
		if event.doubleclick:
			SteamAPI.unlock_achievement(\"RTFM\", true)
		elif event.pressed:
			toggle()

func _input(event: InputEvent) -> void:
	if not is_visible_in_tree():
		return
	
	if event is InputEventJoypadButton:
		if event.button_index == JOY_BUTTON_Y and event.pressed:
			toggle()

func _process(delta):
	if expnded:
		content.size.y = lerp(content.size.y, max_content_size, 0.1)
		if is_equal_approx(content.size.y, max_content_size):
			ultimate_tooltip.show()
			content.size_flags_vertical = 3
			notes.scroll_active = true
			set_process(false)
	else:
		content.size.y = lerp(content.size.y, 0.0, 0.1)
		if is_equal_approx(content.size.y, 128.0) :
			set_process(false)

func toggle():
	expnded = not expnded
	set_process(true)
	
	if expnded:
		content.remove_theme_stylebox_override(\"panel\")
		await get_tree().idle_frame
		content.size.y = 0
		content.size_flags_vertical = 1
		notes.scroll_active = false
	else:
		content.add_theme_stylebox_override(\"panel\", content.stylebox)
		ultimate_tooltip.hide()
		await get_tree().idle_frame
		content.size.y = max_content_size
		notes.scroll_active = false
"

