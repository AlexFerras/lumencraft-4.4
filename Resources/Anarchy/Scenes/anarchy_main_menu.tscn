[sub_resource type="GDScript" id="90"]
resource_name = "GenerationSettings"
script/source = "extends VBoxContainer

var applying: bool

func _ready() -> void:
	$\"%Advanced\".hide()
	apply_presets()
	
	$\"%GenMapSize\".connect(\"value_changed\", Callable(self, \"reset_length\"))
	$\"%GenEnemies\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	$\"%GenTreasures\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	$\"%GenWaveCount\".connect(\"value_changed\", Callable(self, \"reset_length\"))
	$\"%GenWavePower\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	$\"%GenWaveTime\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	$\"%GenWaveTime\".connect(\"value_changed\", Callable(self, \"reset_length\"))
	$\"%GenObjectiveDifficullty\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	$\"%GenObjectiveDifficullty\".connect(\"value_changed\", Callable(self, \"reset_length\"))
	$\"%GenResourceRate\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	$\"%GenStartingResources\".connect(\"value_changed\", Callable(self, \"reset_difficulty\"))
	
	$\"%GenBiome\".assign_options([\"Random\", \"Cave\", \"Tundra\"], [-1, 0, 1])
	$\"%GenBiome\".current_option = 1
	$\"%GenObjective\".assign_options([\"Random\", \"Survive Waves\", \"Collect Resource\", \"Collect Item\", \"Reach Destination\", \"Kill Enemy\", \"Get Crystal\"], [-1, 0, 1, 2, 3, 4, 5])
	$\"%GenObjective\".current_option = 1

func apply_presets():
	applying = true
	
	var difficulty_level: int = -1
	# TODO
	#var difficulty_group = $\"%GenDifficulty\".group
	#if difficulty_group.get_pressed_button():
	#	difficulty_level = difficulty_group.get_pressed_button().get_index()
	
	var wave_time_multiplier := 0.0
	
	match difficulty_level:
		0:
			$\"%GenEnemies\".value = 0.5
			$\"%GenWavePower\".value = 0.5
			$\"%GenTreasures\".value = 1.5
			$\"%GenObjectiveDifficullty\".value = 10
			$\"%GenStartingResources\".value = 800
			$\"%GenResourceRate\".value = 1.3
			wave_time_multiplier = 1.2
		1:
			$\"%GenEnemies\".value = 1.0
			$\"%GenWavePower\".value = 1.0
			$\"%GenTreasures\".value = 1.0
			$\"%GenObjectiveDifficullty\".value = 50
			$\"%GenStartingResources\".value = 0
			$\"%GenResourceRate\".value = 1.0
			wave_time_multiplier = 1.0
		2:
			$\"%GenEnemies\".value = 1.5
			$\"%GenWavePower\".value = 1.5
			$\"%GenTreasures\".value = 0.5
			$\"%GenObjectiveDifficullty\".value = 70
			$\"%GenStartingResources\".value = 0
			$\"%GenResourceRate\".value = 0.7
			wave_time_multiplier = 0.8
	
	var length_level: int = -1
	length_level = 0
	# TODO
	#var length_group = $\"%GenLength\".group
	#if  length_group.get_pressed_button():
		#length_level =  length_group.get_pressed_button().get_index()
	
	match length_level:
		0:
			$\"%GenMapSize\".set_value_if_not_disabled(2048)
			$\"%GenWaveCount\".set_value_if_not_disabled(15)

		1:
			$\"%GenMapSize\".value = 4096
			$\"%GenWaveCount\".value = 30
			$\"%GenObjectiveDifficullty\".value += 10
			if wave_time_multiplier == 0:
				wave_time_multiplier = 1
		2, 3:
			$\"%GenMapSize\".set_value_if_not_disabled(5120)
			$\"%GenWaveCount\".set_value_if_not_disabled(50)
			$\"%GenObjectiveDifficullty\".value += 30

	
	if wave_time_multiplier != 0:
		$\"%GenWaveTime\".value = wave_time_multiplier
	
	applying = false

func reset_length(d):
	if applying:
		return
	
	if $\"%GenLength\".group.get_pressed_button():
		$\"%GenLength\".group.get_pressed_button().button_pressed = false

func reset_difficulty(d):
	if applying:
		return
	
	if $\"%GenDifficulty\".group.get_pressed_button():
		$\"%GenDifficulty\".group.get_pressed_button().button_pressed = false

func randomize_settings() -> void:
	if not Music.is_game_build():
		$\"%GenObjective\".set_option(1 + randi() % ($\"%GenObjective\".options.size() - 1))
		$\"%GenObjectiveDifficullty\".value = randf_range($\"%GenObjectiveDifficullty\".min_value, $\"%GenObjectiveDifficullty\".max_value)
	$\"%GenBiome\".set_option(1 + randi() % ($\"%GenBiome\".options.size() - 1))
	$\"%GenMapSize\".set_value_if_not_disabled(randf_range($\"%GenMapSize\".min_value, $\"%GenMapSize\".max_value))
	$\"%GenWaveCount\".set_value_if_not_disabled(randf_range($\"%GenWaveCount\".min_value, min($\"%GenWaveCount\".max_value, 50)))
	$\"%GenEnemies\".value = randf_range($\"%GenEnemies\".min_value, $\"%GenEnemies\".max_value)
	$\"%GenWavePower\".value = randf_range($\"%GenWavePower\".min_value, $\"%GenWavePower\".max_value)
	$\"%GenWaveTime\".value = randf_range($\"%GenWaveTime\".min_value, $\"%GenWaveTime\".max_value)
	$\"%GenTreasures\".value = randf_range($\"%GenTreasures\".min_value, $\"%GenTreasures\".max_value)

func _on_Endless_toggled(button_pressed: bool) -> void:
	$\"%GenWaveCount\".disabled = button_pressed
"

