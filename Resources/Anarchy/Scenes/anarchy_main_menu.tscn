	max_maps = maps
	set_process(true)
	show()

func _process(delta: float) -> void:
	$Label.text = tr(label_text) % [max_maps - SteamAPI.downloading_maps, max_maps]
	if SteamAPI.downloading_maps == 0:
		hide()
		set_process(false)
