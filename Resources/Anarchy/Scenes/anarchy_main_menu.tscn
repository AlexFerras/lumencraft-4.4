[sub_resource type="GDScript" id="54"]
resource_name = "Leaderboards"
script/source = "extends VBoxContainer

const SCORE_COUNT = 11
var LEADERBOARD_LIST = []

enum {STANDARD, SPEEDRUN}
enum {GLOBAL, NEAR_ME, FRIENDS}

@onready var list := $\"%ScoreList\"

@onready var map_label := $\"%CurrentMap\" as Control
@onready var sort_label := $\"%ScoreSorting\" as Control
@onready var type_label := $\"%ScoreType\" as Control

@onready var prev_page_button := $\"%ScorePrevPage\" as Button
@onready var next_page_button := $\"%ScoreNextPage\" as Button

var leaderboard_handle: int

var score_offset := 0

var wait_avatar := {}

func _ready() -> void:
	if not SteamAPI.active:
		return
	
	for i in SCORE_COUNT:
		list.add_child(preload(\"res://Resources/Anarchy/Scenes/UIElements/ScoreElement.tscn\").instantiate())
		clear_score(i)
	
	var map_list: Array
	var map_names: PackedStringArray
	for leaderboard in LEADERBOARD_LIST:
		map_list.append(leaderboard[1].get_file().get_basename())
		map_names.append(leaderboard[0])
	
	map_label.assign_options(map_names, map_list)
	type_label.assign_options(PackedStringArray([\"Standard\", \"Speedrun\"]), [STANDARD, SPEEDRUN])
	sort_label.assign_options(PackedStringArray([\"Global\", \"Near Me\", \"Friends\"]), [GLOBAL, NEAR_ME, FRIENDS])
	
	SteamAPI.connect(\"requested_scores\", Callable(self, \"on_loaded_scores\"))
	SteamAPI.singleton.connect(\"avatar_loaded\", Callable(self, \"on_avatar_loaded\"))
	update_page()

func load_leaderboard():
	for i in SCORE_COUNT:
		clear_score(i)
	$\"%Loading\".text = \"Loading...\"
	
	map_label.disabled = true
	sort_label.disabled = true
	type_label.disabled = true
	prev_page_button.disabled = true
	next_page_button.disabled = true
	
	var leaderboard_path: String = map_label.get_option()
	if type_label.get_option() == SPEEDRUN:
		leaderboard_path += \"/speedrun\"
	SteamAPI.load_leaderboard(leaderboard_path, 1 + score_offset * SCORE_COUNT, sort_label.get_option(), SCORE_COUNT)

func on_loaded_scores(scores):
	$\"%Loading\".text = \"\"
	
	map_label.disabled = false
	sort_label.disabled = false
	type_label.disabled = false
	prev_page_button.disabled = false
	next_page_button.disabled = false
	
	if scores.is_empty():
		return
	
	prev_page_button.disabled = scores[0].global_rank == 1
	next_page_button.disabled = scores.size() < SCORE_COUNT + 1
	
	for i in min(SCORE_COUNT, scores.size()):
		var score: Dictionary = scores[i]
		set_score(i, score.global_rank, score.score, score.steam_id, score.details)

func clear_score(idx: int):
	list.get_child(idx).clear()

func set_score(idx: int, global_rank: int, score: int, user_id: int, details: Array):
	if type_label.get_option() == SPEEDRUN:
		list.get_child(idx).set_score(str(global_rank), null, SteamAPI.get_nick(user_id), str(details[0]), \"%01d:%02d:%02d\" % [score / 3600, score / 60 % 60, score % 60], details[1])
	else:
		list.get_child(idx).set_score(str(global_rank), null, SteamAPI.get_nick(user_id), str(score), \"%01d:%02d:%02d\" % [details[0] / 3600, details[0] / 60 % 60, details[0] % 60], details[1])
	SteamAPI.get_avatar(user_id)
	wait_avatar[user_id] = list.get_child(idx).get_node(\"%Avatar\")

func get_score_label(idx: int) -> Node:
	return get_child(idx + 1)

func on_avatar_loaded(id: int, width: int, data: PackedByteArray): ## TODO: cache?
	var image := Image.new()
	image = Image.create_from_data(width, data.size() / 4 / width, false, Image.FORMAT_RGBA8, data)
	var texture := ImageTexture.new()
	texture = ImageTexture.create_from_image(image)
	wait_avatar[id].texture = texture

func update_page():
	$\"%PageNumber\".text = tr(\"Page %s\") % (score_offset + 1)
#
#func _menu_option(option: String):
