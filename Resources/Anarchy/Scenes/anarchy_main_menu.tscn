[sub_resource type="GDScript" id="117"]
script/source = "extends Node

const SESSION_FILE = \"user://pregenerated_queue.lcqueue\"

var active: bool

var map_queue: Array
var queue_cursor: int

func _ready() -> void:
	$\"%RandomDifficulty\".hide()

func prepare():
	$\"%RandomStart\".hide()
	$\"%RandomDifficulty\".show()
	
	var file :FileAccess= Utils.safe_open(Utils.FILE, SESSION_FILE, FileAccess.READ)
	if file:
		map_queue = file.get_var()
		queue_cursor = file.get_var()
	else:
		var dir :DirAccess= Utils.safe_open(Utils.DIR, \"res://Maps/Pregenerated\")
		
		dir.list_dir_begin() # TODOConverter3To4 fill missing arguments https://github.com/godotengine/godot/pull/40547
		
		var s := dir.get_next()
		while s:
			map_queue.append(s)
			s = dir.get_next()
		
		map_queue.shuffle()
		save_queue()
	
	active = true

func get_map() -> String:
	var map = map_queue[queue_cursor]
	
	queue_cursor += 1
	if queue_cursor == map_queue.size():
		map_queue.size()
		queue_cursor = 0
	save_queue()
	
	return \"res://Maps/Pregenerated/%s\" % map

func save_queue():
	var file = Utils.safe_open(Utils.FILE, SESSION_FILE, FileAccess.WRITE)
	file.store_var(map_queue)
	file.store_var(queue_cursor)
"

