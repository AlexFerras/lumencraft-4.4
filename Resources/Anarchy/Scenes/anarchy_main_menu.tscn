[sub_resource type="GDScript" id="87"]
script/source = "extends Label

var target_alpha := 0.0
func _ready():
	if SteamAPI.singleton:
		SteamAPI.request_stats()
	
	if Music.is_game_build():
		queue_free()
		return
	
	$Label.text = \"Waiting for stats\"
	#SteamAPI.singleton.connect(\"user_stats_received\", Callable(self, \"update_user_stats\"))
	#SteamAPI.singleton.connect(\"global_stats_received\", Callable(self, \"update_global_stats\"))
		
func _physics_process(delta):
	if SteamAPI.user_stats_received and SteamAPI.global_stats_received:
		update_user_stats(0,0,0)
		update_global_stats(0,\"\")
		set_physics_process(false)

func update_user_stats(game_id: int, result: int, user_id: int):
#	prints(\"updated achievements\")
	$Label.text = \"Achievement stats:\"
	text = str(\"\\nbuildings:\", SteamAPI.achievements.BuildingsBuilt)
	text += str(\"\\nturrets:\", SteamAPI.achievements.TurretsBuilt)
	text += str(\"\\ngates:\", SteamAPI.achievements.GatesBuilt)
	text += str(\"\\n------------------\")
	text += str(\"\\nmetal:\", SteamAPI.achievements.MetalCollected, \" (\", floor(SteamAPI.achievements.AvgMetalPerHour),\"/h, \", round(SteamAPI.achievements.AvgMetalPerMinute),\"/min)\")
	text += str(\"\\nlumen:\", SteamAPI.achievements.LumenCollected, \" (\", floor(SteamAPI.achievements.AvgLumenPerHour),\"/h, \", round(SteamAPI.achievements.AvgLumenPerMinute),\"/min)\")
	text += str(\"\\n------------------\")
	text += str(\"\\nmax techs researched:\", SteamAPI.achievements.MaxResearch)
	text += str(\"\\nfabricated items:\", SteamAPI.achievements.FabricatedItems)
	text += str(\"\\nshredded items:\", SteamAPI.achievements.ShreddedItems)
	text += str(\"\\nkills:\", SteamAPI.achievements.KilledBugs)
	text += str(\"\\nkilled bosses:\", SteamAPI.achievements.KilledBosses)
	text += str(\"\\ndeaths:\", SteamAPI.achievements.Deaths)
	text += str(\"\\nclones:\", SteamAPI.achievements.Clones)
	text += str(\"\\norbs:\", SteamAPI.achievements.Orbs)
	text += str(\"\\nwins:\", SteamAPI.achievements.GamesWon)
	text += str(\"\\nloses:\", SteamAPI.achievements.GamesLost)

func update_global_stats(game_id: int, result: String):
	text += str(\"\\nGwins:\", SteamAPI.achievements.GlobalGamesWon)
	text += str(\"\\nGloses:\", SteamAPI.achievements.GlobalGamesLost)
#	text += str(\"\\nGmetal:\", SteamAPI.achievements.AvgMetalPerHour)
#	text += str(\"\\nGlumen:\", SteamAPI.achievements.AvgLumenPerHour)
func _on_Achievements_visibility_changed():
	update_user_stats(0,0,0)
	update_global_stats(0,\"\")

func _process(delta):
	modulate.a = lerp(modulate.a, target_alpha, 0.1)
	if modulate.a == target_alpha:
		set_process(false)

func _on_mouse_entered():
	target_alpha = 1.0
	set_process(true)

func _on_mouse_exited():
	target_alpha = 0.0
	set_process(true)
"

[sub_resource type="GDScript" id="113"]
script/source = "extends Tree

const BASE_PATH = \"user://Saves\"

func _ready() -> void:
	if not visible:
		$Timer2.stop()
		return
	
	_on_Timer2_timeout()

func list_directory(path: String, item: TreeItem):
	item.set_text(0, path.get_file())
	
	var dir: DirAccess = Utils.safe_open(Utils.DIR, path)
	if not dir:
		return
	
	dir.list_dir_begin() # TODOConverter3To4 fill missing arguments https://github.com/godotengine/godot/pull/40547
	var file := dir.get_next()
	
	while file:
		var item2 := create_item(item)
		
		if dir.current_is_dir():
			list_directory(path.path_join(file), item2)
		else:
			item2.set_text(0, file)
		
		file = dir.get_next()
	
	dir.list_dir_end()

func _on_Timer2_timeout() -> void:
	clear()
	var root := create_item()
	list_directory(BASE_PATH, root)
"

