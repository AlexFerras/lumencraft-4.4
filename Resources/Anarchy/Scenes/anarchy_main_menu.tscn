[sub_resource type="GDScript" id="52"]
resource_name = "PlayGame"
script/source = "extends VBoxContainer

@export var shiny: ShaderMaterial

@onready var list := $\"%MapList\"

var workshop_maps: Dictionary

func _ready() -> void:
	setup_skirmish_map(\"FearMap\", \"Your reactor is out of power and the monsters are creeping in the shadows, waiting for your mistake. Keep your reactor powered with Lumen and use light to scare off the monsters. Brave into darkness to find special golden Lumen that gives infinite energy. Can you complete this challenge with its own unique rules?\", 2)
	
	setup_skirmish_map(\"map_challange_T001\", \"You are in a vast cave system with only a reactor. Your task is to build a base using the rich resource deposits in the area and fend off the monsters that are waiting around every corner.\", 1)
	setup_skirmish_map(\"TrueBeginningMap\", \"Plan your base from scratch to defend against waves from all sides. Explore the caves to find rich resource deposits and valuable treasures.\", 2)
	setup_skirmish_map(\"DemoMap\", \"You take control of an old base. Your aim is to restore its functions and defend from monsters swarming in nearby caves. Something big is lurking in the shadows...\", 2)
	
	# TODO
	#setup_skirmish_map(\"Tunnels\", \"\", 3)
	#setup_skirmish_map(\"Complex\", \"\", 3)
	#setup_skirmish_map(\"DesertEscavation\", \"\", 3)
#	setup_skirmish_map(\"TundraEscavation\", \"\", 3)
	
	#setup_skirmish_map(\"rocket_arena\", \"\", 1)
	setup_skirmish_map(\"FlareSiege\", \"You are alone against a legion of enemies. All you have are flares.\", 1)
	
	if Music.is_demo_build():
		$\"%BaseDefenseHeader\".text = \"Demo Maps\"
		get_tree().call_group(\"disable_in_demo\", \"set_disabled\", true)
		get_tree().call_group(\"disable_in_demo\", \"set_tooltip\", \"Not available in demo\")
		
		return
	
	if SteamAPI.initialized:
		for map in SteamAPI.workshop_maps:
			on_map_downloaded(map)
	
	var dir :DirAccess= Utils.safe_open(Utils.DIR, \"user://maps\")
	if dir:
		dir.list_dir_begin() # TODOConverter3To4 fill missing arguments https://github.com/godotengine/godot/pull/40547
		
		var file_name := dir.get_next()
		while file_name:
			if file_name.get_extension() == \"lcmap\":
				add_custom_map(\"user://maps/\" + file_name, false)
			file_name = dir.get_next()
	
	var last_map_button = Utils.get_prev_sibling($\"%WorkshopContainer\")
	if SteamAPI.initialized:
		last_map_button.focus_neighbor_bottom = last_map_button.get_path_to($\"%BrowseWorkshop\")
	elif not OS.has_feature(\"mobile\"):
		last_map_button.focus_neighbor_bottom = last_map_button.get_path_to($\"%MenuButton9\")

func setup_skirmish_map(path: String, description: String, difficulty: int, wip := false):
	var display_name: String
	var map_button = list.get_node(path)
	
	if FileAccess.file_exists(str(\"res://Maps/\", path, \".lcmap\")):
		path = str(\"res://Maps/\", path, \".lcmap\")
		var map := MapFile.new()
		map.load_metadata(path)
		display_name = map.map_name
		description = map.map_description
	else:
		display_name = Const.MapNames[path]
		path = str(\"res://Maps/\", path, \".tscn\")
	
	if Save.player_data.is_map_completed(path):
		map_button.icon = preload(\"res://Resources/Anarchy/Textures/Icons/flag_64x64.png\")
	
	setup_map_button(map_button, path, display_name, description, true)
	map_button.set_meta(\"difficulty\", difficulty)
	
	$\"%Leaderboards\".LEADERBOARD_LIST.append([display_name, path])
	
	if wip and Music.is_game_build():
		map_button.hide()

func setup_map_button(map_button: Button, path: String, display_name: String, description: String, ranked := false):
	map_button.small_text = display_name
	map_button.set_meta(\"description\", description)
	map_button.set_meta(\"ranked\", ranked)
	
	map_button.connect(\"pressed\", Callable(owner, \"start_game\").bind(path, ranked))
	map_button.connect(\"focus_entered\", Callable(owner, \"show_map_description\").bind(map_button))
	map_button.connect(\"mouse_entered\", Callable(owner, \"show_map_description\").bind(map_button))
	map_button.connect(\"focus_exited\", Callable(owner, \"hide_map_description\").bind(map_button))
	map_button.connect(\"mouse_exited\", Callable(owner, \"hide_map_description\").bind(map_button))

func open_local_map() -> void:
	Utils.open_folder(ProjectSettings.globalize_path(\"user://maps\"))

func add_custom_map(path: String, is_workshop: bool) -> Button:
	var button := $\"%DemoMap\".duplicate() as Button
	button.remove_meta(\"difficulty\")
	
	if is_workshop:
		$\"%WorkshopContainer\".add_child(button)
	else:
		$\"%LocalContainer\".add_child(button)
	
	var display_name: String
	
	var map := MapFile.new()
	map.load_metadata(path)
	if map.map_name.is_empty():
		display_name = path.get_file()
	else:
		display_name = map.map_name
	
	setup_map_button(button, path, display_name, map.map_description)
	if map.validated:
		button.material = shiny
		button.tooltip_text = \"The map is validated, i.e. it's confirmed to be winnable.\"
	
	return button

func on_map_downloaded(map: Dictionary):
	var button := add_custom_map(map.path, true)
	workshop_maps[map.id] = button

func on_map_unsubscribed(id: int):
	if id in workshop_maps:
		workshop_maps[id].queue_free()
		workshop_maps.erase(id)
"

[sub_resource type="GDScript" id="51"]
resource_name = "LoadGame"
script/source = "extends \"res://Resources/Anarchy/Scripts/SaveLoadMenu.gd\"

var try_load: String
var is_deleting: bool
var to_delete: String

func click_slot(slot: String):
	if owner.is_queued_for_deletion(): # syf
		return
	
	if is_deleting:
		if to_delete != slot:
			to_delete = slot
			$\"%DeleteSlotLabel\".text = \"Select again to confirm.\"
		else:
			var error: int
			error = OS.move_to_trash(ProjectSettings.globalize_path(\"user://Saves/\" + slot))
			
			if error == OK:
				list.get_node(slot).queue_free()
			stop_delete()
		
		return
	
	if list.get_node(slot).get_meta(\"game_version\") < VERSION - 100:
		if not $\"%VersionWarning\".visible:
			$\"%VersionWarning\".show()
			try_load = slot
			return
		elif try_load != slot:
			$\"%VersionWarning\".hide()
			try_load = slot
			get_tree().create_timer(0.2).connect(\"timeout\", Callable($\"%VersionWarning\", \"show\"))
			return
	
	if not Music.is_switch_build():
		Utils.set_meta(\"loading_background\", $\"%3DStuff\")
	
	var loading = preload(\"res://Scenes/ScreenLoading.tscn\").instantiate()
	Utils.add_child(loading)
	await RenderingServer.frame_post_draw
	loading.queue_free()
	
	Save.load_game(slot)
#	Save.story_mode = menus.prev_submenu == \"Story\"
	owner.prepare_loading()

func delete_slot() -> void:
	if is_deleting:
		stop_delete()
		return
	
	$\"%DeleteSlotLabel\".show()
	$\"%DeleteSlotLabel\".text = \"Select slot to delete.\"
	to_delete = \"\"
	is_deleting = true

func stop_delete():
	is_deleting = false
	$\"%DeleteSlotLabel\".hide()
	$\"%DeleteSlot\".call_deferred(\"grab_focus\")

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		if not is_visible_in_tree():
			$\"%DeleteSlotLabel\".hide()
			to_delete = \"\"
"

[sub_resource type="GDScript" id="55"]
resource_name = "Workshop"
script/source = "extends VBoxContainer

const PAGE_SIZE = 14
const SORT_TYPES = {Top = 0, Date = 1, Hot = 3, \"Friend Favorite\"= 4, \"Friend Created\"= 5, Controversial = 6, \"Followed Creator\"= 7, \"Not Rated\"= 8, \"Total Votes\"= 9, Subscriptions = 12, \"Recently Updated\"= 19}

const MAP_TYPES = [\"Any\", \"Base Defense\", \"Adventure\", \"Challenge\", \"Story\", \"Other\"]
const MAP_DIFFICULTIES = [\"Any\", \"Casual\", \"Normal\", \"Hardcore\"]
const MAP_LENGTHS = [\"Any\", \"Short\", \"Normal\", \"Long\"]

@onready var map_list := $\"%WorkshopList\"
@onready var map_details := $\"%WorkshopDetails\"
@onready var downloader: HTTPRequest = $\"%Downloader\"
@onready var loading_icon: Sprite2D = $\"%LoadingRotator\"

@onready var play_button: Button = $\"%PlayMapButton\"
@onready var subscribe_button: Button = $\"%SubscribeButton\"
@onready var fav_button: TextureButton = $\"%FavButton\"

var tag_prefab: PackedScene

var query_page: int
var page: int
var map_count: int
var cached: bool

var item_cache: Array

var current_item: Dictionary
var image_cache: Dictionary
var vote_result_pending: bool

var current_map_type := \"Any\"
var current_difficulty := \"Any\"
var current_length := \"Any\"

func _ready() -> void:
	if not SteamAPI.active:
		return
	
	tag_prefab = Prefab.create($\"%TagPrefab\")
	
	DirAccess.make_dir_recursive_absolute(\"user://workshop/preview_cache\")
	
	SteamAPI.connect(\"workshop_loaded\", Callable(self, \"on_workshop_loaded\"))
	SteamAPI.connect(\"map_downloaded\", Callable(self, \"on_map_downloaded\"))
	SteamAPI.singleton.connect(\"get_item_vote_result\", Callable(self, \"on_vote_result\"))
	SteamAPI.singleton.connect(\"set_user_item_vote\", Callable(self, \"on_vote_result2\"))
	
	$\"%WorkshopSort\".assign_options(SORT_TYPES.keys(), SORT_TYPES.values())
	$\"%FilterMapType\".assign_options(MAP_TYPES, MAP_TYPES)
	$\"%FilterDifficulty\".assign_options(MAP_DIFFICULTIES, MAP_DIFFICULTIES)
	$\"%FilterLength\".assign_options(MAP_LENGTHS, MAP_LENGTHS)
	
	for i in PAGE_SIZE:
		var element = preload(\"res://Resources/Anarchy/Scenes/UIElements/WorkshopElement.tscn\").instantiate()
		element.connect(\"pressed\", Callable(self, \"map_selected\").bind(i))
		map_list.add_child(element)

	map_details.hide()

func query_items(from := 1):
	query_page = from
	if from == 1:
		clear()
		refresh_page_buttons()
	
	var tags: Array
	if current_map_type != \"Any\":
		tags.append(current_map_type)
	if current_difficulty != \"Any\":
		tags.append(\"Difficulty: \" + current_difficulty)
	if current_length != \"Any\":
		tags.append(\"Length: \" + current_length)
	SteamAPI.load_workshop($\"%WorkshopSort\".get_option(), query_page, $\"%WorkshopFilter\".text, tags)

func clear():
	for item in map_list.get_children():
		item.clear()
	
	set_details({})
	image_cache.clear()
	page = -1
	map_count = 0

func refresh_page_buttons():
	$\"%WorkshopPrev\".disabled = page == 0
	$\"%WorkshopNext\".disabled = (page + 1) * PAGE_SIZE >= map_count

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		if is_visible_in_tree():
			query_items()

func on_workshop_loaded(count: int, c: bool):
	map_count += count
	cached = c
	
	for i in count:
		var item = SteamAPI.singleton.getQueryUGCResult(SteamAPI.workshop_handle, i)
		item.metadata = SteamAPI.singleton.getQueryUGCMetadata(SteamAPI.workshop_handle, i)
		item_cache.append(item)
		if not item.is_empty():
			item.preview_url = SteamAPI.singleton.getQueryUGCPreviewURL(SteamAPI.workshop_handle, i)
	
	if page == -1:
		set_page(0)
	else:
		refresh_page_buttons()

func set_page(idx: int):
	page = idx
	$\"%WorkshopPage\".text = tr(\"Page %s\") % (idx + 1)
	
	if (page + 2) * PAGE_SIZE >= map_count:
		query_items(query_page + 1)
		$\"%WorkshopNext\".disabled = true
	else:
		call_deferred(\"refresh_page_buttons\")
	
	for i in PAGE_SIZE:
		var j: int = i  + page * PAGE_SIZE
		if j < item_cache.size():
			map_list.get_child(i).set_item(item_cache[j])
		else:
			map_list.get_child(i).clear()
	
	refresh_selected()

func refresh_selected():
	if item_cache.is_empty():
		set_details({})
		return
	set_details({})

func next_page() -> void:
	set_page(page + 1)

func prev_page() -> void:
	set_page(page - 1)

func map_selected(idx: int):
	set_details(map_list.get_child(idx).item)

func set_details(item: Dictionary):
	current_item = item
	
	if current_item.is_empty():
		map_details.hide()
		return
	
	map_details.show()
	play_button.disabled = get_map_path().is_empty()
	subscribe_button.disabled = false
	refresh_subscribe()
	
	if current_item.file_id in SteamAPI.favorited:
		fav_button.set_pressed_no_signal(true)
	else:
		fav_button.set_pressed_no_signal(false)
	
	$\"%WorkshopMapName\".text = current_item.title
	$\"%WorkshopDescription\".text = current_item.description
	vote_result_pending = true
	refresh_votes(0)
	SteamAPI.singleton.getUserItemVote(current_item.file_id)
	
	for tag in $\"%Tags\".get_children():
		tag.queue_free()
	for tag in $\"%InternalTags\".get_children():
		tag.queue_free()
	
	for tag in item.tags.split(\",\"):
		if tag.is_empty():
			break
		
		var tag_label = tag_prefab.instantiate()
		if tag.begins_with(\"Difficulty: \"):
			tag_label.text = str(tr(\"Difficulty\"), \": \", tr(tag.get_slice(\": \", 1)))
		elif tag.begins_with(\"Length: \"):
			tag_label.text = str(tr(\"Length\"), \": \", tr(tag.get_slice(\": \", 1)))
		else:
			tag_label.text = tag
		$\"%Tags\".add_child(tag_label)
	
	for tag in item.metadata.split(\",\"):
		if tag.is_empty():
			break
		
		var tag_label = tag_prefab.instantiate()
		tag_label.text = Utils.get_internal_tag_name(tag)
		$\"%InternalTags\".add_child(tag_label)
	
	if current_item.file_id in image_cache:
		$\"%MapPreview\".texture = image_cache[current_item.file_id]
		return
	
	loading_icon.hide()
	downloader.cancel_request()
	
	if cached:
		var f:FileAccess = Utils.safe_open(Utils.FILE, get_preview_path(), FileAccess.READ)
		if f:
			f.close()
			download_complete(-9999, 0, PackedStringArray(), PackedByteArray())
			return
	
	$\"%MapPreview\".texture = null
	loading_icon.show()
	downloader.download_file = get_preview_path()
	downloader.request(item.preview_url)

func get_preview_path() -> String:
	return str(\"user://workshop/preview_cache/\", current_item.file_id)

func get_map_path() -> String:
	for map in SteamAPI.workshop_maps:
		if map.id == current_item.file_id:
			return map.path
	return \"\"

func open_workshop():
	OS.shell_open(\"https://steamcommunity.com/sharedfiles/filedetails/?id=\" + str(current_item.file_id))

func subscribe() -> void:
	if current_item.subscribed:
		SteamAPI.unsubscribe_map(current_item.file_id)
	else:
		SteamAPI.subscribe_map(current_item.file_id)
		subscribe_button.disabled = true
	current_item.subscribed = not current_item.subscribed
	refresh_subscribe()

func play_map() -> void:
	var path = get_map_path()
	if path.is_empty():
		push_error(\"Map not downloaded\")
	else:
		owner.start_game(path)

func on_map_downloaded(map: Dictionary):
	if map.id == current_item.get(\"file_id\", -1):
		subscribe_button.disabled = false
		play_button.disabled = false

func sort_changed(value) -> void:
	item_cache.clear()
	page = 0
	query_items()

func download_complete(result: int, response_code: int, headers: PackedStringArray, body: PackedByteArray) -> void:
	image_cache[current_item.file_id] = SteamAPI.load_preview_image(get_preview_path())
	$\"%MapPreview\".texture = image_cache[current_item.file_id]
	loading_icon.hide()

func refresh_subscribe():
	subscribe_button.small_text = \"Unsubscribe\" if current_item.subscribed else \"Subscribe\"

func refresh_votes(flags: int):
	$\"%UpvoteButton\".modulate.a = 0.3 if vote_result_pending else 1.0
	$\"%DownvoteButton\".modulate.a = 0.3 if vote_result_pending else 1.0
	$\"%UpvoteButton\".set_pressed_no_signal(bool(flags & 1))
	$\"%DownvoteButton\".set_pressed_no_signal(bool(flags & 2))
	$\"%DetailsUpvote\".text = str(current_item.get(\"votes_up\", 0))
	$\"%DetailsDownvote\".text = str(current_item.get(\"votes_down\", 0))

func upvoted() -> void:
	vote_result_pending = true
	refresh_votes(0)
	SteamAPI.singleton.setUserItemVote(current_item.file_id, true)

func downvoted() -> void:
	vote_result_pending = true
	refresh_votes(0)
	SteamAPI.singleton.setUserItemVote(current_item.file_id, false)

func on_vote_result(result: int, file_id: int, up: bool, down: bool, none: bool):
	if result == 1 and file_id == current_item.get(\"file_id\", -1):
		vote_result_pending = false
		var flags = 0
		flags |= 1 * int(up)
		flags |= 2 * int(down)
		refresh_votes(flags)

func on_vote_result2(result: int, file_id: int, upvote: bool):
	if result == 1 and file_id == current_item.get(\"file_id\", -1):
		vote_result_pending = false
		var flags = 0
		flags |= 1 * int(upvote)
		flags |= 2 * int(not upvote)
		refresh_votes(flags)

func favorite_toggled(fav: bool) -> void:
	if fav:
		SteamAPI.singleton.addItemToFavorites(SteamAPI.APP_ID, current_item.file_id)
		SteamAPI.favorited.append(current_item.file_id)
	else:
		SteamAPI.singleton.removeItemFromFavorites(SteamAPI.APP_ID, current_item.file_id)
		SteamAPI.favorited.erase(current_item.file_id)

func filter_changed(new_text: String) -> void:
	item_cache.clear()
	page = 0
	query_items()

func map_type_changed(option: String) -> void:
	current_map_type = option
	item_cache.clear()
	page = 0
	query_items()

func difficulty_changed(option: String) -> void:
	current_difficulty = option
	item_cache.clear()
	page = 0
	query_items()

func length_changed(option: String) -> void:
	current_length = option
	item_cache.clear()
	page = 0
	query_items()
"

