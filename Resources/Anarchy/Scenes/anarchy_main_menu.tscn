func reset_page():
	score_offset = 0
	update_page()

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		if is_visible_in_tree():
			load_leaderboard()

func prev_page() -> void:
	score_offset -= 1
	update_page()
	load_leaderboard()

func next_page() -> void:
	score_offset += 1
	update_page()
	load_leaderboard()

func map_changed(value) -> void:
	reset_page()
	load_leaderboard()

func sorting_changed(value) -> void:
	reset_page()
	load_leaderboard()

func type_changed(value) -> void:
	reset_page()
	load_leaderboard()
